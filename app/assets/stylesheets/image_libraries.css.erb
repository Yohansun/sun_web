.name_form{position:fixed;top:50%;left:50%;width:400px;margin-left:-200px;height:200px;margin-top:-100px;border:1px solid #ccc;background:#fff;display:none;}
.name_form h3{border-bottom:1px solid #ccc;line-height:30px;padding:0 10px;}
.name_form fieldset{padding:20px 60px;}
.name_form_label{font-size:16px;}
.name_form_label input{height:20px;font-size:16px;width:160px;}
.name_form_button{text-align:center;margin:10px 0;}
.name_form_button a{padding:3px 14px 4px;}

