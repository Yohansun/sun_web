.slider_bin{background:url(<%= image_path "imgbase/loading.gif" %>) no-repeat center center;width:200px;height:150px;overflow:hidden;position:relative;}
.slider_container{width:9999999px;position:relative;}
.slider_container img{float:left;display:inline;}
.slider_bin ul{position:absolute;bottom:10px;right:10px;}
.slider_bin ul li{background:#fff;color:#000;font-size:12px;float:left;display:inline;width:20px;height:20px;text-align:center;line-height:20px;border-radius:10px;cursor:pointer;margin-right:5px;opacity:0.6;filter:alpha(opacity=60);}
.slider_bin ul li.curr{background:#000;color:#fff;}
.slider_prev,.slider_next{width:30px;height:30px;position:absolute;top:50%;z-index:1000;left:10px;text-indent:-999em;}
.slider_next{background-position:-32px 0;left:auto;right:10px;}

#feb_slides{width:686px;height:427px;background:#ececec;}  
/*#feb_slides img{max-width:686px;}*/  
#feb_slides .slider_prev,#feb_slides .slider_next{background-image:url(<%= image_path "news/app_pointer.png" %>);width:32px;height:52px;top:34%;}  
#feb_slides ul{display:none;}  
.slides_container {width:686px;height:427px;}  
.slides_container div {width:686px;height:427px;display:block;}  