@charset "utf-8";

.main{background:url(<%= asset_path "october/october_bg.jpg"%>) no-repeat;width:899px;height:763px;margin:70px auto 50px;padding:396px 32px 30px;}
.october{background:#fff;width:819px;height:700px;padding:48px 40px 15px;}
.october h1,.october a,.october .fr{background:url(<%= asset_path "october/october_items.jpg"%>) no-repeat;text-indent:-999em;}
.october h1{width:470px;height:80px;margin:0 auto 10px;}
.october .fl{width:424px;height:518px;}
.october .pr{background-position:0 -80px;width:308px;height:200px;text-indent:0;padding:19px 17px;margin-top:30px;}
.october h2,.october p,.october p strong{font-weight:700;}
.october h2{color:#ec0303;font-size:18px;margin:30px 0;}
.october p{margin:30px 0;line-height:1.5;color:#848484;}
.october p strong{color:#4d4d4d;}
.october p span{color:#f14646;}
.october a{display:block;width:95px;height:31px;}
.october a.october_find{background-position:-350px -80px;}
.october a.october_upload{background-position:-350px -115px;}
.october a.october_back{background-position:-350px -150px;clear:both;margin:0;}
.october .pr .pa{background:url(<%= asset_path "october/october_png.png"%>) no-repeat;width:90px;height:99px;top:-7px;right:-7px;text-indent:-999em;}
.october p.cl{margin:0;}