.owners_channel{overflow:hidden}
.owners_channel .banner{margin:35px 0}
.owners_channel .intro p{margin:10px 80px;font-size:14px;color:#666}
.owners_form .title{height:32px;border-bottom:4px solid #e3e3e3}
.owners_form .title h5,.owners_answer_form .title h5{float:left;margin-right:10px;width:92px;height:100%;text-indent:-1000em;background:#ffa800 url(<%= asset_path "design/our_needs.gif" %>)}
.owners_form .title p{line-height:28px;font-size:12px;color:#999}
.owners_form .title p b{color:#000}
.owners_form form{margin:20px }
.owners_form table th,
.owners_form table td{padding:5px;font-size:12px;color:#666}
.owners_form table th{width:90px;text-align:right}
.owners_form table .f_content th,
.owners_form table .f_content td{vertical-align:top}
.owners_form textarea{width:600px;height:100px}
.owners_form .f_submit input{vertical-align:top}
.owners_form .reset{padding:0;border:none;width:61px;height:31px;text-indent:-1000em;background:url(<%= asset_path "design/resetB.gif" %>)}

.owners_form.formB .title h5{width:91px;background-image:url(<%= asset_path "design/hon.gif" %>)}
.owners_form.formB .f_input{width:600px;border:1px solid #a7a7a7}
.owners_form.formB .f_select_wrap{float:left;padding:2px 16px 2px 2px;border:1px solid #a7a7a7;background:url(<%= asset_path "design/arr_bottom.gif" %>) no-repeat right center}
.owners_form.formB .f_select_wrap div{height:16px;overflow:hidden}
*+html .owners_form.formB .f_select_wrap div{height:18px;}
.owners_form.formB .f_select_wrap span{display:block;margin:-1px -17px -1px -1px}
* html .owners_form.formB .f_select_wrap div{float:left}

.dec_answer_form .owners_form{margin:20px 0}
.dec_answer_form .owners_form .title h5{width:91px;background-image:url(<%= asset_path "design/answer.gif" %>)}
.dec_answer_form .owners_form form{margin:0;width:600px;font-size:12px;color:#666}
.dec_answer_form .owners_form form p{margin:10px 0;zoom:1}
.dec_answer_form .owners_form form p:after{content:'';display:block;clear:both}
.dec_answer_form .owners_form .input{float:left}
.dec_answer_form .owners_form .submit{float:right}
.dec_answer_form .owners_form .submit input{vertical-align:top}
.dec_answer_form .owners_form .quote{width:240px}
.dec_answer_form .owners_form .quote.hint{color:#ccc}

.form_target{position:absolute;left:-1000px}

.answer_list_wrap{margin:20px 0 50px}
.answer_list_wrap .legend{margin:10px 0;padding:0 5px;height:35px;line-height:35px;font-size:20px;border-top:1px dotted #7a786b}
.answer_list .item{padding:15px 10px 45px;border:1px solid #d7d7d7;border-bottom-color:#efefef}
.answer_list .item .bar{height:20px}
.answer_list .item .bar .user{float:left;font-size:14px;font-weight:bold}
.answer_list .item .bar .user a{color:#999}
.answer_list .item .bar .time{float:right;color:#8c909c;font-size:12px}
.answer_list .item .texts{margin:0 0 0 80px;color:#666;line-height:1.5;font-size:13px}
.answer_list .item .like{float:right;width:75px;height:29px;background:#dadada url(<%= asset_path "design/like_btn.gif" %>)}
.answer_list .item .like a{display:block;height:100%;line-height:29px;font-size:12px;color:#333;text-align:center}

.popupA,.popupA_mask{position:fixed;left:0;top:0;z-index:10;width:100%;height:100%;display:none}
.popupA_mask{background:#000;opacity:0.5;filter:alpha(opacity=50)}
* html .popupA,* html .popupA_mask{position:absolute}
.popupA{z-index:11}
.popupA_content{margin:150px auto 0;border:6px solid #666;background:#fff}
.popupA_content.popupA_middle{width:490px}
.popupA .popupA_title{height:37px;overflow:hidden;border-bottom:1px solid #e6e6e6;background:#f0f0f0 url(<%= asset_path "design/popupA.gif" %>) 0 -21px}
.popupA .popupA_title h4{float:left;padding:0 10px;font-weight:bold;font-size:18px;line-height:37px;color:#959595}
.popupA .popupA_title .close{float:right;margin:8px 10px;width:21px;height:21px;cursor:pointer;background:#9d2c00 url(<%= asset_path "design/popupA.gif" %>)}
.popupA .popupA_body{padding:20px;text-align:center}
.popupA .success{display:inline-block;padding:5px 5px 5px 40px;font-size:22px;color:#c27c02;background:url(<%= asset_path "design/successA.gif" %>) no-repeat 0 center}
*+html .popupA .success{width:1%;white-space:nowrap}
* html .popupA .success{width:1%;white-space:nowrap}
.popupA .desc{margin:5px 0;color:#666;font-size:14px}
.popupA .desc a{text-decoration:underline;color:#666}
.popupA .button{margin-top:15px;padding-top:12px;border-top:1px dotted #bababa}
.popupA .button .okA{width:78px;height:31px;border:none;color:#712323;font-size:14px;font-weight:bold;background:#f5b201 url(<%= asset_path "design/buttonA.gif" %>)}


.listA{width:100%;font-size:14px}
.listA thead th{padding:5px 0;border-bottom:4px solid #e3e3e3;color:#404040}
.listA .c_title{text-align:left}
.listA .c_time{text-align:right}
.listA .c_count{text-align:center}
.listA tbody,.listA tbody a{color:#666}
.listA tbody td{padding:15px 0;border-bottom:1px dotted #000}

.accordionA{}
.accordionA dl{margin-bottom:10px;border:1px solid #d6d6d6}
.accordionA dt{height:32px;border-bottom:1px solid #ccc;cursor:pointer;background:#fdfdff url(<%= asset_path "design/accordionA.gif" %>)}
.accordionA dt strong{float:left;margin-left:10px;padding-left:20px;font-size:14px;font-weight:bold;line-height:32px;color:#b80000;background:url(<%= asset_path "design/accordionA.gif" %>) no-repeat 0 -100px}
.accordionA dt span{float:right;width:44px;height:31px;border-left:2px solid #e8e9eb;border-right:1px solid #fff;border-bottom:1px solid #fff;background:#e9eaec url(<%= asset_path "design/accordionA.gif" %>) repeat-x -14px -30px}
.accordionA dd{padding:10px 10px 10px 30px;color:#696969;background:#f7f7f7 url(<%= asset_path "design/accordionA.gif" %>) no-repeat 10px -123px}
.accordionA dd,.accordionA dd p{line-height:1.8}
.accordionA dl.back{height:32px;overflow:hidden}
.accordionA dl.back span{background-position:-14px -65px}
.accordionA dl.back dd{display:none}


.wrapper{overflow:hidden}

.listA_wrap{margin:30px 0 20px}
.pag_container{margin:18px 0 13px 0;padding:22px 0;border:1px solid #E0E0E0;background:#F2F2F2}
.weekly_tips{background:url(<%= asset_path "news/weekly_tips.jpg" %>) no-repeat left top;padding:100px 50px 0px 38px;}

.oc_pager{position:relative;zoom:1}
.oc_pager:after{content:"";display:block;clear:both}
.oc_pager a.fr{background:#fff;color:#7f7f7f;border:1px solid #d1d1d1;font-size:12px;padding:0.4em 0.5em;margin:0 0 0 1px;}
.oc_pager a.history{width:129px;margin:0 10px 0 2px;}
.historys{width:141px;height:140px;overflow:auto;right:34px;bottom:2px;border:1px solid #d1d1d1;}
.historys a{display:block;background:#fff;color:#7f7f7f;padding:0.2em 0.5em;border:1px solid #d1d1d1;}
a.next{padding-right:15px!important;}
a.prev{padding-left:15px!important;}
a.history{width:130px;}
a.history span{display:block;position:absolute;top:6px;border-color:#737373 transparent;_line-height:0;font-size: 0px;right:8px;border-width:5px;border-style:none solid solid solid;height:0px;}

.weekly_tips{margin-bottom:30px;padding-bottom:20px;border-bottom:1px solid #e0e0e0}

.dec_qc .title{margin:15px 0;padding:12px 0;border-bottom:4px solid #e3e3e3;font-size:18px;font-weight:bold}
.dec_qc .dec_question{margin:15px 0;padding:10px;border:1px solid #d6d6d6}

.dec_question .titlebar{margin:5px 0;height:18px;}
.dec_question .legend{float:left;padding-left:20px;height:18px;font-size:14px;font-weight:bold;background:url(<%= asset_path "design/question.gif" %>) no-repeat left center}
.dec_question .legend a{color:#999;font-weight:bold}
.dec_question .titlebar .time{float:right;margin-right:10px;font-size:12px;color:#8c909b}
.dec_question_content{margin:15px 0;font-size:12px}
.dec_question_content strong{float:left;width:90px;color:#c97908;text-align:right;font-weight:bold}
.dec_question_content .text{margin:0 0 15px 90px;line-height:1.5;color:#666}
.dec_question_content .image{margin-left:90px}

