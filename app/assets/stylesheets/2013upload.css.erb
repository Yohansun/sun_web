/*mood*/
.channel_bottom {margin-top:40px;}
#bottom_mood{width:530px;}
#bottom_mood h4{font-size:14px;font-weight:bold;margin-top:20px;}
.mood_textarea{background:url(<%= asset_path "channel/mood_textarea_bg.jpg" %>) repeat-x;border:1px solid #c8cace;height:110px;width:496px;margin:10px 0;padding:5px;}
.btn_group{padding-right:22px;}
.btn_group li{position:relative;margin-right:15px;cursor:pointer;font:12px/18px "Simsun";color:#505050;}
.btn_group li ul{/*display:none;*/position:absolute;top:17px;left:0;z-index:20;}
.btn_group li ul li a{font:12px/18px "Simsun";color:#595959;/*display:block;*/}
.btn_group li.btn_pictures{width:45px;height:23px;overflow: hidden;cursor: pointer;}
.btn_group li.btn_pictures input{position:relative;/*top:-20px;*/left:0;/**left:-54px;*/width:45px;filter:alpha(opacity=0);opacity:0;cursor: pointer;}
.btn_group li.btn_pictures span{position:absolute;/**left:30px;padding:1px 25px;*/cursor: pointer;*width:60px;}
.btn_group li.btn_send{margin-right:0;}
.btn_send button{background:#ffde5c;border:1px solid #ecbeb8;width:65px;line-height:22px;text-align:center;color:#9d5201;}
.btn_send a.buttons{background:#ffde5c;border:1px solid #ecbeb8;width:65px;line-height:22px;text-align:center;color:#9d5201;}
.btn_group li.btn_public{width:50px;}
.btn_group li.btn_public ul{/*display:none;*/position:absolute;width:60px;border:1px solid #cccccc;background:#ffffff;}
.btn_group li.btn_public ul li{overflow:hidden;margin:1px;}
.btn_group li.btn_public ul li a{display:block;padding:3px;line-height:22px;}
.btn_group li.btn_public ul li a:hover{background:#e6e6e6;}

.color_mood_options{width:160px;border:1px solid #cccccc;padding:5px;background:#ffffff;display:none;}
.color_mood_options li{float:left;width:18px;height:13px;border:1px solid #fff;text-align:center;margin:0px;padding:3px 0;}
.color_mood_options li a{width:12px;height:12px;display:block;margin:0 auto;border:1px solid #ccc;padding:0;}
.color_mood_options li.hover{border:1px solid #9a9afb;}

.mood_list{border-top:1px solid #ccc;margin:20px 20px 28px 0;}
.mood_list li{border-bottom:1px dashed #ccc;padding:15px 0;}
.mood_list li strong{font-weight:bold;color:#e71d37;font-size:12px;}
.mood_list li p{margin:10px 0;line-height:16px;}
.mood_list li span,.mood_list li span a{color:#acacac;}

.btn_group i{float:left;}
