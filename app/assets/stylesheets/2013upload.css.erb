@charset "utf-8";

.upload_content select,
.upload_content textarea,
.upload_content input[type="text"],
.upload_content input[type="password"],
.upload_content input[type="datetime"],
.upload_content input[type="datetime-local"],
.upload_content input[type="date"],
.upload_content input[type="month"],
.upload_content input[type="time"],
.upload_content input[type="week"],
.upload_content input[type="number"],
.upload_content input[type="email"],
.upload_content input[type="url"],
.upload_content input[type="search"],
.upload_content input[type="tel"],
.upload_content input[type="color"],
.upload_content .uneditable-input{border-radius:0;}
.upload_content fieldset{border-bottom:1px solid #f3f3f3;padding:15px;}
.upload_content .control-label{width:140px;}
.upload_content .controls{margin-left:160px;*margin-left:0px;}
.upload_content .control-group{margin-bottom:0;}
.upload_content .checkbox,.upload_content .radio,.upload_more .checkbox,.upload_more .radio{margin-left:0;margin-right:10px;white-space:nowrap;font:12px "SimSun",Arial;}
.upload_content .radio.inline + .radio.inline,.upload_content .checkbox.inline + .checkbox.inline{margin-left:0;}
.control-label.pt10{padding-top:10px;}
.upload_tab.nav-tabs{background-color:#f8f8f8;margin-bottom:0;}
.upload_tab.nav-tabs li{margin-bottom:0;}
.upload_tab.nav-tabs > li > a{-webkit-border-radius:0;-moz-border-radius:0;border-radius:0;border:0;color:#000;border-top:2px solid #f8f8f8;}
.upload_tab.nav-tabs > li.active > a,.upload_tab.nav-tabs > li.active > a:hover{border:2px solid #ff6300;border-width:2px 0 0;}
#workname,#building_name,#design_idealins_title,#ins_title,.workname,.building_name{height:28px;box-shadow:inset 0px 1px 3px #ddd;width:354px;}
.design_ideal{height:140px;width:755px;}
.upload_img_btn{display:inline-block;width:112px;height:40px;background:url(<%= asset_path '2013/btns_icon2.png'%>) no-repeat 0 -243px;border:0;color:#8f6012;margin:25px 0 20px;font-size:18px;line-height:40px;text-align:center;}
.upload_img_btn:hover{color:#8f6012;}
.upload_img_back{display:inline-block;padding:5px;height:26px;width:65px;line-height:27px;border:1px solid #dddfe2;background:#fefefe;text-align:center;font-size:18px;color:#5c6572;border-radius:1px;margin-left:10px;*margin-top:-54px;}
.upload_img_back:hover{color:#5c6572;}
/*upload form1---ins*/
.ins_upload_form{padding:50px 0 0;}
.ins_upload{border-top:1px solid #f3f3f3;}
/*upload form2---ins*/
.ins_upload_form2{padding:10px 30px;}
.ins_upload_form2 .upload_img_btn{margin:10px 0;}
.ins_uploadimg_list .load_content{height:110px;overflow:hidden;}
.ins_uploadimg_list .work_title{width:735px;}
/*upload form2*/
.uploadimg_list,.ins_uploadimg_list{padding-top:15px;margin-top:17px;border-top:1px solid #e6e6e6;}
.load_bar{border-bottom:3px solid #ededed;padding:0px 0 10px;}
.work_size{width:88px;color:#666;}
.set_cover{width:88px;color:#666;}
.work_del{width:82px;color:#666;}
.load_content{height:235px;overflow:auto;padding-right:20px;}
.work_title{width:560px;}
.file_list{border-bottom:1px dotted #000;padding:15px 0;}
.work_title img{margin:0px 20px;width:120px;height:90px;}
.file_name{margin:36px 0 0;}
.file_list .work_size,.file_list .set_cover,.file_list .work_del{padding:36px 0;}
a.load_del{padding:0.1em 1em;border:1px solid #dfdfdf;background:#f8f8f8;color:#000;}
p.load_footer,p.load_total{margin:15px 0 8px 20px;color:#666;}
p.load_info{margin:0 0px 4px 20px;color:#666;}
.clear_all{color:#666666;}

.upload_item{padding:36px 0 16px 20px;margin-top:20px;border-top:1px solid #ddd;}
