@charset "utf-8";

.upload2013{border-bottom:1px solid #e6e6e6;padding:25px 20px;}
.upload2013 h1,.control2013 input,.control2013 textarea{font:bold 14px 'Simsun';}
.control2013{margin:10px 0;}
.control2013:after{content:"";display:block;clear:both;}
.label2013{font-size:14px;width:120px;text-align:right;float:left;display:inline;margin:15px;}
.control2013 input{height:38px;color:#000;font-size:18px;*line-height:38px;margin-right:15px;}
.width652{width:652px;}
.width335{width:335px;}
.help-text{margin:16px 0;font-size:12px;color:#c5c5c5;}
