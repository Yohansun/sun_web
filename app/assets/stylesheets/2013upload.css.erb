.channel_list li p span{color:#e71d36;}

.submit_form{min-height:496px;*height:600px;background:url(<%= asset_path "channel/submit_form_bg.jpg"%>) no-repeat right 111px;*background-position:right 190px;border-bottom:1px solid #e6e6e6;}
.submit_form h3{border-bottom:1px solid #e6e6e6;padding-top:80px;position:relative;height:30px;}
.submit_form h3 img{position:absolute;top:32px;left:35px;height:88px;}
.submit_form .form_legend{width:50%;text-align:center;font:25px "Microsoft Yahei";color:#4a4a4a;padding:30px 0 20px;margin-bottom:30px;border-bottom:1px dashed #f2f2f2;}
.submit_form .form_legend span{display:block;font-size:16px;}
.submit_form .form_list{margin-bottom:30px;}
.submit_form fieldset{width:50%;margin:12px;*padding:0 0 10px;font:12px "Simsun";}
.submit_form label{float:left;width:100px;text-align:right;margin-right:10px;line-height:26px;}
