.submit_form input,.submit_form select{border:1px solid #d2d4d4;}
.submit_form input{width:140px;padding:3px;height:18px;}
.submit_form select{height:27px;}
fieldset.deshed_line{border-top:1px dashed #f2f2f2;/*margin-top:30px!important;*/padding-top:25px;}
.channel_banner{padding:35px 0;border-bottom:1px solid #e6e6e6;}
/*pagebar*/
.sub_bot .pager{margin-top:28px;}
.pager{/*width:388px;*/text-align:center;margin:0 auto; padding:3px 0;_padding:4px 0;}
.pager a{background:#fff;color:#7f7f7f;padding:0.2em 0.5em;border:1px solid #d1d1d1;}
.pager a:hover,.pager a.current{color:#bb0e0e;_display:inline;}

a.prev{padding-left:15px;position:relative;}
a.next{padding-right:15px;position:relative;}
a.prev span,a.next span{display:block;position: absolute;top:6px;border-color:transparent #737373;_line-height:0;font-size: 0px;}
a.prev span{left:3px;border-width:5px;border-style: solid solid solid none;height:0px;_border-color:#fff #737373;_filter:chroma(color=#ffffff);}
a.next span{right:3px;_left:50px;border-width:5px;border-style: solid none solid solid;height:0px;_border-color:#fff #737373;_filter:chroma(color=#ffffff);}
/*pagebar ends*/
/*tabnav style*/
.channel{margin-top:30px;}
.tabnav{background:#f8f8f8;border-bottom:1px solid #e6e6e6;/*overflow:hidden;*/font-size:14px;}
.tabnav li{float:left;padding:25px;border-top:3px solid transparent;border-right:1px solid #e6e6e6;cursor:pointer;}
.tabnav .current{border-top:3px solid #4e4a9e;background:#fff;font-weight:bold;color:#000;border-right-color:#fff;}
