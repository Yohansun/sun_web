.indexkv_r{width:270px!important;}
.ul_dot li{overflow:hidden!important;white-space:normal!important;}
.index_access .ul_dot li p a:hover{color:#444444;}

.index_tit strong{width:auto!important;}
.index_tit a{width:auto!important;}
.tagbase{width:160px!important;}
