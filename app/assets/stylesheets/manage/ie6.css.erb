.navbar .nav li{float:left;}
.navbar .nav li a{
	float: none;
	padding: 10px 15px 10px;
	color: #ffffff;
	text-decoration: none;
	text-shadow: 0 1px 0 #ffffff;
	display:block;
}
.navbar .nav .active a{
	color: #ffffff;
	text-decoration: none;
	background-color: #cf0921;
}
.subnav{top:36px!important;}
.navbar .subnav li a,.navbar .subnav li a:hover{font:12px/16px "SimSun";color:#797979!important;padding:10px 2px;}
.navbar .subnav li a:hover{color:#fe6601!important;font-weight:bold;}
.top_search input{overflow:hidden;}
.nav_top{width:730px!important;}
#indexkv,#master_slide{overflow:hidden;}
