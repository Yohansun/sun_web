/*###################################### refresh list begin #################################################*/
.refresh_lists .map-left{height:999px;width:758px;}
.refresh_lists .map-right{height:auto;}
.refresh_lists .media.media_chann{margin-top:6px;}
.refresh_lists .media.media_chann:first-child{margin-top:0px;}
.refresh_lists .refresh_list li{background:transparent;width:auto;margin:0 0 7px;border-bottom:0;}
.refresh_lists .refresh_list p a{display:block;line-height:18px;}
.refresh_lists .tit_con{border-bottom:1px solid #efefef;padding-bottom:20px;margin:15px 15px 16px;}
.refresh_list .btn_detail{padding:5px 12px;background:#fb5240;border-radius:3px;margin:0 9px;color:#fff;}

.refresh_news{padding:15px;}
.refresh_news .ul_dot{list-style-type:disc;}
.refresh_news .ul_dot a,.refresh_lists .map-right li a{color:#474747;}
.refresh_new{width:358px;border:1px solid #e6e6de;margin:0px 9px 0px 0;}
.refresh_new ul{margin-bottom:10px;padding:0 8px;}
.refresh_new li{padding:0 5px;}
.refresh_new h5{padding:9px 12px;margin:0;background:#f0f0f0;border-bottom:1px solid #e6e6de;color:#474747;}
.refresh_main .footer_banner,.refresh_main .index_map{margin:10px 0 0;}
