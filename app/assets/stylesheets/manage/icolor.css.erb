.map-right .refresh_r_tit{padding-left:0;}
.refresh_video .map-right a{color:#474747;}
.refresh_video .map-left{height:728px;width:755px;padding:2px;}
.refresh_video .refresh_bg{background:url(<%= asset_path "refresh_life/refresh/refresh_video_bg.jpg" %>) no-repeat center top;height:565px;}
.refresh_video .refresh_l_tit{color:#003788;font:bold 24px "Microsoft Yahei";padding:20px 10px 16px;margin:0 16px 28px;border-bottom:1px dotted #adadad;}
.refresh_video .refresh_l_tit span{color:#6b6b6b;font-size:14px;}
.refresh_video .video{/*padding:3px;background:#fff;width:594px;*/margin:36px 0 40px 82px;}
.icon-refresh_tel{width:24px;height:24px;line-height:24px;background:url(<%= asset_path "manage/icons.png" %>) no-repeat;background-position:0 -120px;}
.refresh_video_btn{display:inline-block;*display:inline;*zoom:1;width:236px;height:51px;background:url(<%= asset_path "manage/btns/btns_icon2.png" %>) no-repeat 0 -62px;}
.refresh_banner{text-align:center;vertical-align:middle;background:#ededee;height:85px;padding:39px 0;}
/*1*/
.refresh_lists.refresh_video1 .map-left{height:578px;}
.refresh_video1 .video_title{padding:10px 0 15px;margin:0 19px 18px;border-bottom:1px solid #efefef;}
