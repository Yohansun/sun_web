.index_map li:hover .icon-map5{background-position:-40px -160px;}
.index_map li:hover .icon-map6{background-position:-40px -200px;}
.index_map li:hover .icon-map7{background-position:-40px -240px;}
.index_map li:hover .icon-map8{background-position:-40px -280px;}
.index_map li:hover .icon-map9{background-position:-40px -320px;}
.icon-i16{width:16px;height:16px;line-height:16px;background:url(<%= asset_path 'manage/icons.png'%>) no-repeat;}
.icon-more{background-position:0 -16px;margin-top:-1px;}
.icon-more_cof{background-position:0 -32px;margin-top:-1px;}
.icon-more_yellow{background-position:0 -48px;margin-top:-1px;}
.icon-xin{background-position:-16px -16px;margin-top:0;}
.icon-xin_gray{background-position:-32px -16px;margin-top:0;}
.noborder{border:0!important;}
.nomargin{margin:0!important;}
.nopadding{padding:0!important;}
