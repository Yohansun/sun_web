/*###################################### refresh video list begin ##################################################*/
.refresh_video_list li.span5{background:transparent;margin:0 0 20px;width:360px;height:430px;padding:0 5px;}
.refresh_video_list .thumbnails{margin:0;padding:0 9px;}
.refresh_video_list .thumbnail{border-radius:0;padding:9px;}
.refresh_video_list .vedio_thumbnail{display:block;width:340px;height:240px;margin-bottom:9px;}
.refresh_video_list .thumbnail h5{margin:0;font-family:"Simsun";padding:0 3px;}
.refresh_video_list .thumbnail p{line-height:18px;padding:9px 0 0;border-top:1px dotted #d2d2d2;margin-top:9px;height:36px;overflow:hidden;}
.refresh_video_list .show_all_link{color:#ff6600;display:inline-block;*display:inline;*zoom:1;line-height:18px;padding:4px 0 0;}
.refresh_video_list .video_share{background:#f6f6f6;border-top:1px dotted #d2d2d2;height:30px;margin:10px -9px -9px;}
.refresh_video_list .video_share #ckepop{margin-top:7px;}
.refresh_video_list .page_title{margin:0 14px 10px;border-bottom:1px dotted #d2d2d2;padding:10px 3px;font:bold 14px "Simsun";color:#474747;}
.refresh_video_list .pager-form.pager{padding:10px 0 0;}
