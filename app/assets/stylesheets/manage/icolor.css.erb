.channel_search{margin:7px 0 0;padding:0;}
.channel_search .search{background:url(<%= asset_path 'manage/channel/search_chann.jpg'%>);width:107px;height:22px;border:0;padding:1px 0;margin:0 5px;}
.channel_search .search input{margin-bottom:0;}
.channel_search .search input.input-medium{width:80px;*width:76px;border:0;background:transparent;padding:4px 3px 4px 6px;}
.channel_btn{background:url(<%= asset_path 'manage/channel/btns/btns_icon2.png'%>) no-repeat 0 -38px;width:67px;height:24px;color:#9d5201;}
.channel_search select{width:108px;*margin-top:0;}
.input_image{margin-top:4px;width:14px;overflow:hidden;}
/*--------channel list*/
.chann_warp.chann2_warp{padding:0;margin-top:20px;}
.chann_warp.chann2_warp .chann_tit,.chann_warp.chann2_warp .channel_search{padding:11px 9px 0;}
.chann_warp.chann2_warp .chann_linebg{margin:0 9px;}
.chann2_warp .pager{padding:12px 0 25px;}
.chann2_warp .lists_text li{float:left;padding:20px 0px;border:1px solid #f2f2f2;border-width:0 0 1px 1px;margin-left:-1px;}
.chann2_warp .item_bg{width:172px;height:200px;overflow:hidden;background:url(<%= asset_path 'manage/channel/icons2.png'%>) 0px -192px;margin:0 14px 0 13px;}
.chann2_warp .item{width:155px;margin:9px 7px 5px 10px;}

.lists_text .item_img{display:table-cell;vertical-align:middle;text-align:center;height:115px;width:155px;/*font-size:100px;*/position:relative;}
.lists_text h6,.lists_text p{overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.lists_text h6,.lists_text h6 a{height:20px;font:bold 12px/20px "SimSun";color:#535353;padding:0;margin:6px 0 2px;}
.lists_text h6 a:hover,.channel3 .lists_text h6 a:hover{color:#ff6600;text-decoration:underline;}
.lists_text p{font:12px/18px "SimSun";color:#7f7f7f;margin:0;padding:0;height:18px;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.lists_text p span{color:#e71d36;}

.channel3 .chann2_left{width:540px;height:725px;}
.channel3 .chann2_right{width:449px;height:725px;}
.channel3 .chann_warp{padding:11px 13px;}
.channel3 hr.border2{background:url(<%= asset_path 'manage/channel/chann_linebg.jpg'%>) no-repeat 0 -2px;height:2px;border:0;}
.channel3 .lists_text{padding:10px 0;border-top:1px solid #ddd;border-bottom:1px dotted #ddd;margin-bottom:13px;}
.channel3 .lists_text li{float:left;width:155px;padding:0 7px 0 8px;height:170px;}
.channel3 .lists_text h6 a,.channel3 .lists_text p{color:#565656;}
.channel_slide{position:relative;*height:238px;}
.channel_slide .slider-container{width:512px;}
.channel_slide .chann_indicators{position:static;list-style:none;text-align:center;margin:0 0 31px;*margin:0px;}
.channel_slide .chann_indicators li{float:none;display:inline-block;*display:inline;*zoom:1;width:9px;*margin-right:4px;}
.channel_slide .chann_indicators a{display:block;width:8px;height:8px;overflow:hidden;background:url(<%= asset_path 'manage/channel/icons.png'%>) 10px 0;text-indent:-99999px;margin:0 2px 0 3px;}
.channel_slide .chann_indicators .slider-active a{background-position:26px 0;}
.channel_passage{height:98px;background:#f6f6f6;margin:30px 10px 10px;}
.channel_passage span{padding:14px 50px 14px 20px;width:60px;height:70px;background:url(<%= asset_path 'manage/channel/icons2.png'%>) no-repeat 83px -409px;font:26px "Microsoft Yahei";color:#bababa;}
.channel_passage a{float:left;margin:27px 6px;padding:10px;height:24px;background:#fff;border-radius:3px;font:18px "Microsoft Yahei";color:#e71d37;}
.channel_passage a:hover,.channel_passage a:hover span{color:#fff;background:#e71d37;text-decoration:none;}
.channel_passage a,.channel_passage a span{display:block;text-align:center;}
.channel_passage a span{font-size:10px;text-transform:uppercase;}
.icon-star_of{position:absolute;top:-1px;right:-1px;*right:2px;width:75px;height:75px;background:url(<%= asset_path 'manage/icons2.png'%>);}
.icon-star_month{background-position:-67px -2px;}
.icon-star_week{background-position:-67px -82px;}

.mood_textarea{background:#fcfcfc url(<%= asset_path 'manage/textarea_bg.jpg'%>) repeat-x 0 0;border:1px solid #c8cace;height:100px;width:408px;margin:0px 0 10px;padding:5px;border-radius:0;font:12px/16px "Microsoft Yahei";}
.moodbtn_group li{position:relative;cursor:pointer;font:12px/18px "Simsun";color:#505050;margin-top:-2px;}
.moodbtn_group li.pull-left{margin-right:15px;margin-top:2px;}
.moodbtn_group li ul{position:absolute;top:17px;left:0px;z-index:20;}
.moodbtn_group li a{color:#595959;}
.moodbtn_group li.btn_pictures{width:45px;height:23px;overflow:hidden;cursor:pointer;}
.moodbtn_group li.btn_pictures input{position:relative;left:0;width:45px;filter:alpha(opacity=0);opacity:0;cursor: pointer;}
.moodbtn_group li.btn_pictures span{position:absolute;cursor:pointer;}
.btn_send button{background:#ffde5c;border:1px solid #ecbeb8;width:65px;line-height:22px;text-align:center;color:#9d5201;}
.moodbtn_group li.btn_public{width:50px;margin-top:2px}
.moodbtn_group li.btn_public ul{position:absolute;width:60px;border:1px solid #cccccc;background:#ffffff;left:-24px;}
.moodbtn_group li.btn_public ul li{overflow:hidden;margin:1px;}
.moodbtn_group li.btn_public ul li a{display:block;padding:3px;line-height:22px;}
.moodbtn_group li.btn_public ul li a:hover{background:#e6e6e6;}
.icon-mood{background:url(<%= asset_path 'manage/channel/icons.png'%>) no-repeat;width:16px;height:16px;line-height:16px;margin-top:-1px}
.icon-color_mood{background-position:2px -64px;}
.icon-self{background-position:-16px -64px;margin-right:5px;}
.icon-face{background-position:-32px -64px;}
.icon-pic{background-position:0 -80px;margin-right:5px;}
.icon-public{background-position:-16px -80px;margin-right:5px;}
.icon-arrow-up{background-position:-32px -80px;}
/*.icon-arrow-down{background-position:-8px -8px;}*/
.color_mood_options{width:144px;border:1px solid #cccccc;padding:5px;background:#ffffff;}
.color_mood_options li{float:left;border:1px solid #fff;text-align:center;margin:0px;padding:2px;}
.color_mood_options li a{width:10px;height:10px;display:block;border:1px solid #a6a6a6;}
.color_mood_options li.hover{border:1px solid #9a9afb;}

.mood_list{border-top:1px solid #ccc;margin:20px 5px 28px;font-size:12px;}
.mood_list li{border-bottom:1px dotted #ccc;padding:5px 3px;}
.mood_list li strong{font-weight:bold;color:#e71d37;font-size:12px;}
.mood_list li p{margin:3px 0 10px;line-height:16px;}
.mood_list li span,.mood_list li span a{color:#acacac;}

.link_mood{font-size:12px;padding:10px 0 24px;*padding-bottom:36px;text-align:right;}
.link_mood a{color:#333333;}
/*###################################### channel end ###################################################*/
/*###################################### refresh begin #################################################*/
.media_refresh{width:477px;padding:0 15px 0 10px;border-right:1px solid #e2e2e2;margin-top:0}
.media_refresh .title{border-bottom:1px dotted #909090;color:#666666;margin-top:0;line-height:28px;height:28px;}
.media_refresh .title a.pull-right{margin-top:10px;}
.media_refresh .media-body li{border-bottom:1px solid #e2e2e2;line-height:27px;height:27px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;padding:0 5px;}
.media_refresh li a{color:#474747;}
.media_refresh .title_opacity{position:relative;}
.media_refresh .title_opacity:hover{*text-decoration:none;}
.media_refresh a.title_opacity p{padding:5px;}
.media_refresh .slider-container{position:relative;}
.media_refresh .slider-container,.media_refresh .item{width:240px;height:170px;}
.media_refresh .slider-container li{padding:0;}

.media_refresh .circle_indicators{position:absolute;right:7px;bottom:7px;}
.circle_indicators{position:static;list-style:none;text-align:center;margin:0 0 31px;*margin:0px;}
.circle_indicators li{float:none;display:inline-block;*display:inline;*zoom:1;width:9px;*margin-right:4px;height:10px;line-height:10px;border:0;}
.circle_indicators a{display:block;width:9px;height:9px;overflow:hidden;background:url(<%= asset_path 'manage/channel/icons.png'%>) -20px -108px;text-indent:-99999px;margin:0 2px 0 3px;}
.circle_indicators .slider-active a{background-position:-3px -108px;}
/*###################################### refresh end ###################################################*/
/*2*/
