/*rebuild*/
.container{width:1000px;}
.navbar .container,.navbar-fixed-top .container{width:1000px;background:#fff;}
.navbar .brand{padding:21px 0 20px;margin:0 0 0 19px;}
/*custom*/
body{background:#f2f2f2 url(<%= asset_path 'manage/bg.jpg'%>);}
a,a:hover{text-decoration:none;}
/*--top*/
.nav_top{position:relative;height:39px;padding:21px 0 0px;font-size:12px;*width:715px;}
.nav_top a{color:#8f8f8f;margin-right:3px;padding-left:3px;}
.top_search{position:relative;width:118px;height:25px;background:url(<%= asset_path 'manage/search.jpg'%>) no-repeat;display:inline-block;*margin-right:5px;}
.top_search input{width:90px;height:16px;padding-right:22px;border:0;background:transparent;border-radius:0;}
.top_search button{position:absolute;top:3px;right:2px;border:0;background:none;width:18px;height:18px;background:url(<%= asset_path 'manage/search-icon.jpg'%>) no-repeat;text-indent:-9999px;}
.nav_top .follow_button{line-height:0;margin-top:2px;}
.nav_top select{height:26px;line-height:26px;border-radius:0;font-size:12px;*height:24px;*line-height:24px;margin-top:-1px;}
.nav_top .btn_gray,
.nav_top .btn_red,
.nav_top .btn_gray:hover,
.nav_top .btn_red:hover{color:#fff;border:0;border-radius:0;/*padding:2px 7px;*/margin-top:0;}
.btn_gray,.nav_top .btn_gray:hover{background:#b5b5b5;}
.btn_red,.nav_top .btn_red:hover{background:#e71d36;}
.nav_top #ckepop{height:20px;overflow:hidden;}
.nav_top .jiathis_area{position:absolute;top:47px;left:324px;width:170px;/*height:26px;*/padding:8px;background:#f3f3f3 no-repeat left top;display:none;}
.nav_top .jiathis_area a{float:left;}
.user_infos{height:20px;line-height:20px;padding-top:28px;margin-right:6px;}
.user_infos a{color:#8f8f8f;font-size:12px;}
.user_infos a strong{display:inline-block;margin-right:5px;width:70px;height:16px;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
/*--nav*/
.nav_red{background:#e71d36;width:100%;}
.nav_red li{position:relative;width:110px;border-left:1px solid #cf0921;text-align:center;}
.nav_red li.w112{width:112px;}
.navbar .nav_red li a,
.navbar .nav_red li a:hover{text-shadow:none;color:#fff;font:bold 14px "Microsoft Yahei";}
.navbar .nav_red .active a,
.navbar .nav_red .active a:hover{background:#cf0921;color:#fff;}
/*subnav*/
.subnav{position:absolute;top:38px;left:0;z-index:9;background:#f2f2f2;border:solid #d6d6d6;border-width:2px 1px 0;border-top-color:#fe6601;list-style:none;margin-left:0;display:none;}
.subnav li{border-bottom:1px #d6d6d6 solid;border-left:0;}
.navbar .subnav li a,.navbar .subnav li a:hover{font:12px/30px "SimSun";color:#797979;}
.navbar .subnav li a:hover{color:#fe6601;font-weight:bold;}
/*--bottom*/
.links,.links a,.footer small{color:#858585;font-size:12px;}
.footer a{font-size:12px;color:#666666;margin:0 7px;}
/*--index kv*/
#indexkv{width:700px;height:380px;margin-bottom:0;}
#indexkv ul{list-style:none;margin:0;}
#indexkv .item{height:345px;width:700px;text-align:center;}
#indexkv .item a{display:table-cell;height:345px;width:700px;overflow:hidden;text-align:center;vertical-align:middle;font-size:284px;*font-size:280px;}
#indexkv .item img{display:inline;}
#indexkv .carousel-indicators{position:static;height:34px;border-bottom:1px solid #e71d36;background:#fff;}
#indexkv .carousel-indicators li{position:relative;/*left:-1px;*/text-indent:0;height:auto;width:20%;padding-top:9px;margin-left:0;text-align:center;background:#fff;border-bottom:5px solid #fff;}
#indexkv .carousel-indicators li a{color:#999999;font-size:12px;display:block;border-left:1px solid #cbcbcb;height:20px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;/*margin:9px 0 0;*/}
#indexkv .carousel-indicators .slider-active{border-bottom:5px solid #e71d36;border-radius:0;}
#indexkv .carousel-indicators a:hover .hover{position:absolute;top:-5px;left:63px;width:16px;height:5px;background:url(<%= asset_path 'manage/icons.png'%>) no-repeat 0 0;}
#indexkv .slider-container{width:700px;height:345px;}
/*--index kv right*/
.indexkv_r{padding:0 15px;background:#fff;height:380px;width:270px;}
.ul_dot{list-style-type:square;list-style-position:inside;margin:0 0 15px 0;}
.ul_dot h5{color:#666666;}
.ul_dot li{border-bottom:1px dotted #ddd;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;height:35px;}
.ul_dot li a{line-height:35px;color:#336699;font-size:12px;}
.ul_dot li a:hover,.ul_dot li:hover{color:#ff6600;}
/*.indexkv_r_img{border:1px solid #ddd;padding:2px;}*/
/*--imagebase*/
.index_tit{margin:0;position:relative;top:2px;padding-top:20px;*top:9px;*padding-top:10px;}
.index_tit strong{border-bottom:2px solid #904323;line-height:24px;}
.index_tit a{color:#333333;margin-left:5px;}
.index_main{border:1px solid #e5e5dd;background:#fff;padding:10px 0;}
.tagbase{width:160px;}
.imgbase_show{width:418px;}
.tagbase dl{padding:0 10px 8px;font-size:12px;margin:0 0 8px;border-bottom:1px dotted #d2d2d2;}
.tagbase dt{margin-bottom:5px;}
.tagbase dt a{color:#848383;font-weight:normal;}
.tagbase dd{width:50%;float:left;margin-left:0;height:20px;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}
.tagbase dd a{color:#333;}
.imgbase_show a{float:left;margin:2px 2px 0 0;position:relative;overflow:hidden;}
.imgbase_show a p{position:absolute;left:0;bottom:0;width:100%;padding:8px 0;margin-bottom:0;color:#333;font-weight:bold;background:rgba(255, 255, 255, 0.7) none repeat scroll 0 0 !important;filter:Alpha(opacity=70); background:#fff;}
.imgbase_show a:hover p{background:rgba(0, 0, 0, 0.7) none repeat scroll 0 0 !important;filter:Alpha(opacity=70); background:#000;color:#fff;}
.imgbase_show a p span{display:block;padding:0 8px;font-size:14px;}
.imgbase_show .show1,.imgbase_show .show1 img{width:408px;height:244px;}
.imgbase_show .show2,.imgbase_show .show2 img{width:244px;height:162px;}
.imgbase_show .show3,.imgbase_show .show3 img{width:162px;height:162px;}
.imgbase_show .show4,.imgbase_show .show4 img{width:162px;height:162px;}
.imgbase_show .show5,.imgbase_show .show5 img{width:244px;height:162px;}
.imgbase_show .show6,.imgbase_show .show6 img{width:244px;height:244px;}
.imgbase_show .show7,.imgbase_show .show7 img{width:162px;height:244px;}
/*--master*/
.index_master .media{width:320px;margin-left:10px;}
/*.index_master .media a.media_a,.index_access .media a.media_a{border:1px solid #eeeeee;padding:2px;}*/
.index_master .media p{color:#333;font:12px/20px "SimSun";max-height:100px;overflow:hidden;}
#master_slide{width:640px;height:125px;margin:15px 10px 15px 0;display:inline;}
#master_slide ul{list-style:none;margin-left:0;}
/*#master_slide li{width:596px;}*/
#master_slide li a{float:left;margin:0 2px;}
#master_slide .slider-prev,#master_slide .slider-next{width:11px;height:129px;background:url(<%= asset_path 'manage/btns/btns_icon1.jpg'%>) no-repeat;border:0;border-radius:0;opacity:1;font-size:0;top:0;margin:0;text-indent:-99999px;}
#master_slide .slider-prev:hover{background-position:-22px 0;}
#master_slide .slider-next{background-position:-11px 0;}
#master_slide .slider-next:hover{background-position:-33px 0;}
#master_slide .slider-container{width:596px;height:125px;margin:0 auto;}
#master_slide .slider-container li{width:149px;height:125px;}
/*--design*/
.design_show{margin:0 0 0 10px;}
.design_show li{list-style:none;border-right:1px solid #e5e5e5;margin:0 26px 0 0;padding-right:17px;width:300px;}
.design_show p{width:300px;height:270px;margin-bottom:0;}
.design_show p a{float:left;margin:0 0px 10px 0;}
.design_show h5{border-bottom:1px dotted #d2d2d2;margin:0 10px 8px 0;padding-bottom:8px;}
.design_show h5 a{color:#333;}
.design_show h5 a.pull-right{color:#666;}
/*--access*/
.index_access{width:670px;height:283px;*height:290px;}
.index_access .media{padding:1px 10px;}
.index_access .ul_dot li{height:auto;white-space:normal;overflow:visible;}
.index_access .ul_dot h5{display: inline;}
.index_access .ul_dot{width:348px;}
.index_access .ul_dot li a{line-height:24px;color:#444444;}
.index_access .ul_dot li:hover h5 a{color:#ff6600;}
.index_access .ul_dot p{height:72px;overflow:hidden;}
/*--news*/
.index_news{width:310px;height:283px;*height:290px;}
.index_news .index_tit a{color:#7b2705;text-decoration:underline;}
.index_news .media{padding:0 10px;}
.index_news .ul_dot li,.index_news .ul_dot li a{line-height:33px;height:33px;}
.index_news .media a.media_a{border:1px solid #eeeeee;width:138px;}
.index_news .media a.media_a h6{line-height:35px;margin:0;padding:0 5px;color:#333;text-align:center;}
/*--footer_banner*/
.footer_banner{/*border:1px solid #e5e5dd;background:#fff;padding:2px;height:94px;*/height:85px;overflow:hidden;margin:20px 0 0;}
/*.footer_banner.pull-left{width:664px;}*/
/*.footer_banner.pull-right{width:304px;}*/
/*--index_map*/
.index_map{list-style:none;margin:20px 0 0;}
.index_map li{float:left;text-align:center;margin:10px 0;width:12.5%;*width:12.47%;}
.index_map a{display:block;text-align:center;color:#666666;font:12px/24px "SimSun";}
.index_map a:hover{color:#ff6600;/*text-decoration:underline;*/}
/*common*/
.icon-map{width:40px;height:40px;line-height:40px;background:url(<%= asset_path 'manage/icons.jpg'%>) no-repeat;}
