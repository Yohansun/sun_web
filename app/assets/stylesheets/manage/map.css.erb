@charset "utf-8";

/*== map ==*/
.map-search{
	font-size: 12px;
	height: 43px;
}
.map-search a{
	color: #999;
}
.map-search p{
	margin: 8px 0;
}
.map-search form{
	margin: 0;
}
input.map-text{
	background: #fff url(<%= asset_path 'manage/search-icon.jpg'%>) no-repeat 5px center;
	padding: 0 0 0 30px;
	height: 29px;
	width: 154px;
	border: 1px solid #bebebe;
	margin: 5px -5px 0 10px;
	border-right: 0;
	border-radius: 4px 0 0 4px;
	line-height: 29px;
	overflow: hidden;
}
input.map-submit{
	background: url(<%= asset_path 'manage/map/map-submit.jpg'%>) no-repeat;
	width: 57px;
	height: 31px;
	border: 0;
	color: #fff;
	font-weight: bold;
	margin-top: 5px;
}
.map-nav{
	background: #474747;
	margin-bottom: 0;
}
.bd{
	border: 1px solid #e5e5dd;
}
.map-nav a{
	display: block;
	padding: 8px 7px;
}
.map-nav .pull-left a{
	color: #fff;
	font-weight: bold;
}
.map-nav .pull-right a{
	color: #979797;
}
.map-top{
	background: #fff;
	border: 1px solid #e6e6de;
	padding: 10px;
}
.map-slider{
	width: 300px;
	border: 0;
	padding: 0;
}
.map-slider .slider-container,.map-slider .item{
	width: 300px;
	height: 410px;
}
.map-cen{
	width: 420px;
	margin-left: 15px;
}
.map ul{
	list-style: none;
	margin-left: 0;
}
.map a{
	color: #777;
}
.map a:hover,.map-search a:hover{
	color: #f60;
	text-decoration: underline;
}
.map-cen .map-title1,.map-cen .map-title2{
	font: bold 18px "Microsoft Yahei";
}
.map-cen h4{
	margin: 18px 0;
}
a.map-title1{
	color: #c00;
}
.map-cen .map-title2{
	color: #262626;
	font-weight: normal;
}
.map-news{
	width: 420px;
	margin-bottom: 25px;
	margin-bottom: 23px\9;
}
.map-news a{
	width: 198px;
	margin: 4px 0;
	vertical-align: middle;
}
.map-news a,.map-bt2{
	display: inline-block;
	white-space: nowrap;
	overflow: hidden;
	text-overflow:ellipsis;
}
.map-news span{
	margin-right: 10px;
	margin-right: 8px\9;
}
.map-bt{
	border-top: 1px solid #dedede;
	padding-top: 20px;
}
.map-bt li{
	margin: 7px 0;
}
.map-bt span{
	margin: 0 6px;
}
.map-bt a{
	color: #2e2e2e;
}
.map-bt .map-bt1{
	color: #777;
}
.map-bt2{
	width: 173px;
	width: 170px\9;
	vertical-align: middle;
}
.map-news span,.map-bt span{
	color: #dedede;
}
.map-r{
	width: 230px;
}
.ft14{
	font-size: 14px;
}
.map-left{
	width: 748px;
	height: 340px;
}
.map-left li{
	background: url(<%= asset_path 'manage/map/map-li.jpg'%>) no-repeat;
	width: 154px;
	height: 143px;
	padding: 9px;
	float: left;
	margin: 0 0 14px 12px;
}
.map-left p{
	line-height: 2.4;
}
.map-left a{
	color: #474747;
}
.map-right{
	width: 210px;
	height: 340px;
	padding: 10px 15px;
}
.map-right li{
	padding: 8px 0 8px 0px;
}
.map-right .text-center{
	background: url(<%= asset_path 'manage/map/map-r.jpg'%>) no-repeat center bottom;
	color: #474747;
	font: bold 14px "Arial";
	margin-bottom: 8px;
	padding-left: 0;
}
.map-r1 a{
	color: #474747;
}
.map-r1 strong,.map-r2 strong,.map-r3 strong,.map-r4 strong,.map-r5 strong{font-weight:normal;cursor:pointer;}
.map-r1.active strong,.map-r2.active strong,.map-r3.active strong,.map-r4.active strong,.map-r5.active strong{font-weight:bold;}
.map-r1 i,.map-r2 i,.map-r3 i,.map-r4 i,.map-r5 i{margin-top:-1px;}
/*.map-r1,.map-r2,.map-r3,.map-r4,.map-r5{
	background: url(<%= asset_path 'manage/map/map-n.jpg'%>) no-repeat 0 10px;
}*/
.icon-1{background-position:-16px -184px;}
.icon-2{background-position:0px -200px;}
.icon-3{background-position:0px -216px;}
.icon-4{background-position:0px -232px;}
.icon-5{background-position:0px -248px;}
.map-r2,.map-r3,.map-r4{
	border-bottom: 1px solid #e0e0e0;
}
.map-r2{
	background-position: 0 -144px;
}
.map-r3{
	background-position: 0 -175px;
}
.map-r4{
	background-position: 0 -207px;
}
.map-r5{
	background-position: 0 -239px;
}
.map-bot li{
	float: left;
	_display: inline;
	margin: 0 27px;
}
.map-bot strong{
	display: block;
	font-size: 14px;
	padding: 7px 0;
	color: #333;
	text-align: center;
	border-bottom: 1px solid #e0e0e0;
	margin-bottom: 5px;
}
.map-bot1{
	width: 145px;
}
.map-bot2{
	width: 220px;
}
.map-bot3{
	width: 130px;
}
.map-bot4{
	width: 150px;
}
.map-bot div{
	width: 50%;
	*width: 49.9%;
}
.map-bot5,.map-bot5 div{
	width: 80px;
}
.map-bot2 div{
	width: 33.3%
}
.map-bot div{
	float: left;
	_display: inline;
	text-align: center;
}
.map-bot p{
	margin: 2px 0;
}

/*== map-list ==*/
.map-list{
	background: #fff;
}
.map-list-nav{
	background: #efefef;
}
.map-list-nav li{
	float: left;
	text-align: center;
	padding: 7px 21px;
	font: bold 14px "Simsun";
	color: #555;
	cursor: pointer;
}
.map-list-nav .list-curr{
	background: #e71d36 url(<%= asset_path 'manage/map/list/list-curr.jpg'%>) no-repeat bottom center;
	color: #fff;
}
.map-list-nav p{
	margin: 7px;
	color: #3b3b3b;
}
.map-list-nav strong{
	color: #f60;
}
.map-list-search{
	background-color: #fff;
	padding: 10px;
  margin-bottom: 20px;
}
.map-srch-bin{
	padding: 10px;
	margin-bottom: 0;
}
.map-list-form{
	border: 1px solid #dbdbdb;
	border-width: 1px 0;
	padding: 5px 0;
}
.map-mini{
	height: 120px;
	overflow: hidden;
}
.map-srch-s{
	background-color: #fff;
	border: 1px solid #f60;
	color: #f60;
	padding: 2px;
	zoom: 1;
	cursor: pointer;
}
.map-srch-del{
	background: url(<%= asset_path 'manage/map/list/srch-del.jpg'%>) no-repeat;
	width: 9px;
	height: 9px;
	display: inline-block;
	vertical-align: middle;
	text-indent: 100%;
	overflow: hidden;
	margin-left: 5px;
}
.map-srch-reset{
	color: #7f7f7f;
}
.map-list-search fieldset{
	padding: 3px 0;
	border-bottom: 1px dashed #dbdbdb;
}
.map-list-search label{
	font-size: 12px;
	padding: 2px 5px;
	color: #333333;
	font-weight: bold;
	margin-bottom: 0;
}
.map-list-search fieldset a{
	padding: 1px 6px;
	line-height: 22px;
	float: left;
	margin: 0 4px 4px;
	color: #757575;
	font-size: 12px;
}
.map-list-search fieldset span{
	margin-left: 75px;
	zoom: 1;
	display: block;
}
.map-list-search a.highlight{
	color: #fff;
	background: #e71d36;
}
.map-list-search select{
	width: 75px;
	padding: 0;
	margin: 0 5px 0 0;
	height: 20px;
}
.map-srch-submit{
	border: 1px solid #c1c6cb;
	background: #fff;
	padding: 0 5px;
	color: #5c6572;
}
.pr{
	position: relative;
}
.pa{
	position: absolute;
}
.map-list-up{
	background: url(<%= asset_path 'manage/map/list/list-toggle.jpg'%>) no-repeat;
	width: 81px;
	height: 22px;
	line-height: 22px;
	bottom: -11px;
	left: 50%;
	margin-left: -40px;
	text-align: center;
	text-indent: 12px;
}
.map-list-down{
	background-position: 0 -22px;
}
.map-flow{
	width: 750px;
}
#map-flow{
	height: 1800px!important;
}
.map-flow-r{
	width: 240px;
}
.map-right .map-r0{
	border-bottom: 1px dotted #909090;
	padding-left: 0;
}
.map-r0 strong{
	float: left;
	font-size: 14px;
}
.map-rel{
	height: 220px;
}
.map-rel li{
	border-bottom: 1px dotted #909090;
	padding-left: 0;
}
.map-rel li a{
	background: url(<%= asset_path 'manage/map/list/map-rel.jpg'%>) no-repeat 8px -28px;
	padding-left: 20px;
}
.map-rel .map-r0 a{
	background: none;
}
.map-rel li a:hover{
	background-position: 8px 5px;
}
#map-flow div{
	border: 1px solid #dbdbdb;
	background-color: #f5f5f5;
	padding: 10px;
	width: 210px;
	margin: 9px;
}
#map-flow p{
	margin: 10px 0 0;
}
.pager-form.pager{
	border-top: 1px dotted #dbdbdb;
	padding: 30px 0 20px;
}
.pager-form input{
	background: url(<%= asset_path 'manage/map/list/pager-input.jpg'%>) no-repeat;
	width: 26px;
	height: 20px;
	line-height: 20px;
	margin: 0 3px;
	padding: 0;
	text-align: center;
	outline: 0;
	font-size: 12px;
}
.pager-submit{
	border: 0;
}

/*== map-big ==*/
.map-big{
	width: 620px;
	height: 388px;
}
.map-kv-slider{
	width: 87px;
	position: relative;
}
.map-kv-slider ul{
	position: relative;
	list-style: none;
	margin: 0 0 0 5px;
	width: 75px;
}
.map-kv-slider li{
	margin: 8px 0;
}
.map-kv-slider a{
	color: #666;
	display: block;
	text-align: center;
	padding: 5px;
}
.map-kv-slider a:hover{
	color: #f60;
	text-decoration: underline;
}
.map-prev,.map-next-bullet{
	border-bottom: 1px solid #d0cfb7;
}
.map-next,.map-top-bullet{
	border-top: 1px solid #fff;
}
.map-next-bullet,.map-prev-bullet{
	background: url(<%= asset_path 'manage/map/in/map-bullet.png'%>) no-repeat;
	text-indent: 100%;
	overflow: hidden;
}
.map-prev-bullet{
	background-position: center 11px;
}
.map-next-bullet{
	background-position: center -13px;
}
.map-kv-container{
	width: 80px;
	height: 340px;
	margin: 8px auto;
	position: relative;
	overflow: hidden;
}
.map-kv-container a{
	position: relative;
	padding: 0;
	width: 80px;
	height: 75px;
	display: block;
}
.map-kv-container span{
	background: url(<%= asset_path 'manage/map/in/map-active.png'%>) no-repeat;
	position: absolute;
	top: 0;
	left: -5px;
	width: 80px;
	height: 75px;
	display: none;
}
.map-kv-container a:hover span,.map-active span{
	display: block;
}