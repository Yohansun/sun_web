.pb{padding-bottom:60px;}
.winner_list,.win_section1,.judge1_list{background:url(<%=asset_path 'winner_list/section1_bg.jpg'%>) repeat;}
.winner_tab.nav-tabs{margin-bottom:0;background:url(<%=asset_path 'winner_list/winner_tabg.jpg'%>) repeat-x;height:57px;border-bottom:0}
.winner_tab.nav-tabs li{color:#d3cdb7;font:20px 'Microsoft Yahei';}
.winner_tab.nav-tabs li a{line-height:41px;border:0;border-radius:0;padding:8px 40px;color:#d3cdb7;}
.winner_tab.nav-tabs li a:hover{background-color:transparent;border:0;}
.winner_tab.nav-tabs li.active a{color:#ddae3e;background:url(<%=asset_path 'winner_list/winner_tab_active.jpg'%>) repeat-x;border:0;}
.winner_list .title{height:106px;background:url(<%=asset_path 'winner_list/section1_tit1.png'%>) no-repeat center top;color:#ddae3e;font:20px/24px 'Microsoft Yahei';}
.winner_list .title small{font:10px 'BradloSlab Regular,Arial';text-transform:uppercase;color:#ddae3e;-webkit-transform:scale(0.8);}
.winner_list li{float:left;height:170px;width:435px;padding-right:11px;background:#202020;margin:0 0 20px 35px;display:inline;}
.winner_list .check_btn{color:#bc953b;font:12px/40px 'SimSun';}
.winner_list .check_btn:hover{text-decoration:underline;}
.winner_list li a.img,.winner_list .name{cursor:default;}
.win_details{margin-left:110px;padding-top:10px;height:160px;overflow:hidden;}
.win_details p{color:#9b9a99;font:12px/18px 'SimSun';height:35px;overflow:hidden;}
.win_details .name{color:#ddae3e;font:18px/24px 'Microsoft Yahei';height:24px;overflow:hidden;}
.win_details h3{font:12px 'SimSun';color:#626262;border-bottom:1px dotted #454545;margin:0 0 3px;}
.auther_img img{width:100px;height:130px;}
.auther_works{height:75px;overflow:hidden;}
.auther_works img{margin-right:5px;float:left;width:75px;height:75px;overflow:hidden;}

.win_lists{padding-left:15px;margin-top:10px;}
.win_pages .title{background:url(<%=asset_path 'winner_list/section2_tit.jpg'%>) no-repeat;margin:0;height:110px;padding-top:22px;color:#fff;}
.win_pages .title small{color:#fff;}
.over_btn{height:40px;line-height:40px;width:100px;background-color:#000000;}
.pager_btn{display:inline-block;*display:inline;*zoom:1;width:62px;height:36px;background:url(<%=asset_path 'winner_list/winner_page_btn.png'%>) no-repeat;margin:0 5px;font:12px "SimSun";color:#000;}
.pager_btn small{line-height:55px;}
.pager_btn.next{background-position:-62px 0;}

.win_section1{padding:20px 0px;}
.win_section2{padding:0 0 1px;background:url(<%=asset_path 'winner_list/section2_bg.jpg'%>) repeat;}
.win_section3{padding:0px;background:#645242;margin:20px 0 0;}
.win_section3 .title{background:url(<%=asset_path 'winner_list/section3_tit.jpg'%>) no-repeat;}

#winner_tab2{border-top:4px solid #2e2b29;}
.judge1_list{padding:44px 30px;}
.judge1_list .win_details{height:240px;padding:0;margin-left:190px;}
.judge1_list .win_details p{line-height:18px;height:auto;margin-bottom:0;}
.judge1_list li{width:440px;height:240px;margin-left:13px;background:url(<%=asset_path 'winner_list/judge_libg.jpg'%>) repeat;}
.judge1_list .name{margin:10px 0 5px;padding:5px 0;border-bottom:1px solid #000000;color:#fff;}

/* gift list */
.gift_list{background: #f7e6d4;padding-bottom: 35px;}
.gift_list .imgs{font-size: 0;line-height: 0;}
.gift_item{width:890px;margin:50px auto 0 auto;padding-bottom: 50px;}
.gift_item .title{height:64px;background:url(<%=asset_path 'winner_list/gift_list/gift_list_tit.png' %>) no-repeat;padding-left:46px;/*font:normal 24px "Microsoft Yahei";color:#ea4b35;*/}
.gift_item .table{width:800px;margin:0 auto;background:#fbfbfb;}
.gift_item .table th{text-align: center;background:#eeeeee;border-bottom:1px solid #cdcdcd;font:normal 14px/30px "SimSun";color: #686868;}
.gift_item .table td{border-top: 0;border-left:1px solid #cdcdcd;}
.gift_item .table .bg_gray{background:#f4f4f4;}
.gift_carousel{width:800px;margin:0 auto;}
.gift_carousel li{width:800px;}
.gift_carousel .slider-prev,
.gift_carousel .slider-next{height:30px;width:30px;line-height:30px;background:url(<%= asset_path 'manage/icons.png'%>) no-repeat;position: absolute;top: 50%;margin-top: -15px;text-indent: -9999px;}
.gift_carousel .slider-prev{left:-48px;background-position:0 -779px;}
.gift_carousel .slider-next{right:-48px;background-position:0 -809px;}
.gift_carousel .slider-prev:hover{background-position:0 -839px;}
.gift_carousel .slider-next:hover{background-position:0 -869px;}