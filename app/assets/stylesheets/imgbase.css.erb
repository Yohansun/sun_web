/*nav bug hack*/
/**/
.nav{background-image:url(<%= asset_path "imgbase/nav.png" %>);width:1194px;}
.navBot li a{padding:8px 18px;_padding:8px 15px;}
.highlight{color:#fff!important;background:#e71d36!important;}
.img_vertical_align{
	width:760px;height:507px;
	text-align:center;
	background:#ececec;

	/* 兼容标准浏览器 */
	display: table-cell;
	vertical-align:middle;

	/* 兼容IE6/IE7 */
	*display:block;
	*font-size:443px;  /* 字体大小约为容器高度的0.873倍 400*0.873 = 349 */
	*font-family:Arial;  /* 防止非utf-8引起的hack失效问题，如gbk编码 */
}

.img_vertical_align img{
	vertical-align:middle;
}

.imgbase{background-color:#fff;width:1180px;margin:0 auto;padding-bottom:20px;}
.imgbase a:hover,.search_bin a.curr_rank,.tit_tab .fresh,.imgbase_title strong{color:#e71d36;}
/**/
.imgbase_title{background:url(<%= asset_path "imgbase/imgbase_bg.jpg" %>) no-repeat center bottom;height:94px;padding:20px 0 0 30px;margin:0 auto;}
.imgbase_title p{font:bold 18px/30px "Microsoft Yahei";color:#131313;padding-left:3px;}
.imgbase_title strong{font-weight:700;}
.imgbase_search{margin-top:5px;}
.imgbase_search button{background:url(<%= asset_path "imgbase/imgbase_btn.jpg" %>);width:44px;height:23px;color:#a94305;font-size:14px;}
.imgbase_search input{height:20px;margin-right:2px;background:#fff url(<%= asset_path "search-icon.jpg" %>) no-repeat 5px center;border:1px solid #d6d6d6;outline:none;width:150px;padding-left:25px;font-size:14px;}
.imgbase_search span{color:#999999;font-size:12px;}
.imgbase_search span a{color:#a66969;padding:0 5px;}
/*main*/
.imgbase_main{background:#fff;margin-bottom:35px;}
.tit_tab{height:40px;background:#efefef;border-top:1px solid #d6d6d6;}
.tit_tab ul li{float:left;line-height:14px;width:56px;height:14px;font-size:14px;color:#555555;display:block;padding:13px 20px;cursor:pointer;}
.tit_tab ul li.active_tit{color:#fff;background:#e71d36 url(<%= asset_path "imgbase/imgbase_arrow.jpg" %>) no-repeat center bottom;}
.tit_tab .fresh{padding:3px 10px;background:#fff;border-radius:5px;font-weight:bold;}
.tit_tab p{background:#fdf6b3;padding:0 10px;}
.tit_tab p a{padding:0 6px;color:#000;font:bold 14px/40px "SimSun";}
.tit_tab p a.active_condition{color:red;}
/*tab content*/
.tab_content{padding:20px 20px 0;font:12px "SimSun";width:1140px;overflow:hidden;border-bottom:1px solid #ebebeb;}
.tab_content label{padding:6px 5px;color:#333333;font-weight:bold;}
.tab_content a{color:#757575;font:12px "SimSun";padding:0 5px;}
.tab_content fieldset a{padding:1px 5px;line-height:22px;float:left;margin:0 5px 4px;}
.tab_content fieldset span{margin-left:65px;display:block}
.tab_content fieldset.lastfield a{float:none;}
.tab_content fieldset{padding:2px 0;border-bottom:1px dashed #e6ccae;}
.tab_content fieldset.lastfield{padding-top:25px;border:none;}
.tab_content select,.tab_content input{border:1px solid #ccc;}
.tab_content select{padding:3px;color:#757575;}
.tab_content input{padding:4px 10px;color:#6e6e6e;font-size:14px;background:#fff url(<%= asset_path "imgbase/imgbase_button.jpg" %>);width:56px;height:25px;border:0;}
.imgbase_toggle{background:url(<%= asset_path "imgbase/arrow.jpg" %>) no-repeat;width:81px;height:22px;line-height:22px!important;bottom:-21px;left:50%;margin-left:-40px;text-align:center;}
.form_expand{background-position:0 -22px;}
.form_mini{height:90px;}

.search_bin{background:#ebebeb;border-top:2px solid #e71d36;padding:15px 20px;}
.search_bin p{color:#333;font:bold 14px 'Arial';margin-right:190px;}
.search_bin a{color:#4a4a4a;font-size:12px;margin-left:10px;}
/*list*/
.imgbase_area{width:1155px;margin:0 auto 25px;border-bottom:1px dashed #e6ccae;padding:10px 0;}
.imgbase_list li,.imgbase_list a{width:224px;height:197px;}
.imgbase_list li{float:left;display:inline;margin-left:7px;}
.imgbase_list a{color:#707070;font-size:12px;position:relative;display:block;}
.imgbase_list img{max-width:224px;max-height:162px;}
.imgbase_list p{margin:8px 0 15px;text-align:center;}
.imgbase_list span{width:218px;height:156px;border:3px solid #e71d36;position:absolute;left:0;top:0;display:none;}
.imgbase_curr span,.imgbase_list a:hover span{display:block;}
.imgbase_curr a{color:#e71d36;}

.imgbase_bin{width:686px;height:579px;margin-left:7px;}
#imgbase_info,.imgbase_collect,.imgbase_fullpage{background:url(<%= asset_path "imgbase/imgbase_icon.jpg" %>) no-repeat;}
#imgbase_info{background-repeat:repeat-x;width:666px;height:60px;padding:10px;font-size:14px;}
#imgbase_info h4{margin:10px 0;}
#imgbase_info h4 a{color:#e71d36;}
#imgbase_info span,#imgbase_info p,.imgbase_like,.imgbase_comm{color:#ededed;}
#imgbase_info span{font-size:12px;}
#imgbase_info p a{color:#e71d36;font-size:12px;text-decoration:underline;}
.imgbase_like,.imgbase_comm{background:url(<%= asset_path "imgbase/imgbase_like.png" %>) no-repeat;margin-left:5px;padding-left:20px;height:16px;font-size:12px;line-height:16px;float:right;display:inline;}
.imgbase_comm{background-position:0 -16px;}

.imgbase_func{right:8px;top:8px;width:340px;height:21px;text-align:right;z-index:10;}
.imgbase_collect,.imgbase_view,.imgbase_fullpage{background-color:#fafbfc;border:1px solid #ccc;border-radius:3px;height:21px;}
.imgbase_collect,.imgbase_fullpage{padding:3px 5px 3px 30px;color:#000;}
.imgbase_collect{background-position:0 -82px;}
.imgbase_fullpage{background-position:0 -103px;}
.imgbase_view{padding:3px 10px;}
#imgbase_view{color:#be0f25;}
.imgbase_view,a.imgbase_view:hover{color:#848484;}


/*imgbase_in*/
h3.tit_tab{border-top:2px solid #e71d36;padding:0 25px;font:18px/36px "Microsoft Yahei";color:#e71d36;}
.imginfo_show{float:left;width:760px;padding:18px 0 0 20px;margin-right:15px;}
.imginfo_show .imgbase_func{top:30px;}
.imginfo_big_img{margin-bottom:15px;}
.link_group{position:absolute;top:500px;right:10px;color:#ededed;font:12px "SimSun";text-decoration:underline;}
.imginfo_big_txt{min-height:42px;_height:42px;font:12px/24px "SimSun";}
.imginfo_big_txt span{margin-right:25px;}
.imginfo_big_txt strong{font-weight:bold;}
.colors_recommend{position:relative;padding:8px 13px;background:#ffffff;border:1px solid #949192;border-left-color:#f0f0f0;margin:12px 0 20px 35px;min-height:90px;_height:90px;}
.colors_recommend strong{position:absolute;left:-30px;top:27px;display:block;width:20px;height:62px;}
.colors_recommend span{/*float:left;*/display:block;width:62px;height:62px;margin-right:15px;}
.colors_recommend p{width:30%;height:62px;}
.colors_recommend p em{/*display:block;bottom:-72px;height:50%;*/font:12px/16px "SimSun";}

h3.img_reply_tit{color:#e71d36;font:16px "Microsoft Yahei";margin:10px 0;}
.img_reply textarea{width:752px;height:100px;padding:3px;font:14px/18px "Microsoft Yahei";margin-bottom:10px;border:1px solid #dbdbdb;}
.img_reply2{margin-left:65px;display:none;}
.img_reply2 h3{padding:15px 0 5px 0;color:#838282;font:12px "SimSun";}
.img_reply2 textarea{width:687px;}
.img_reply2 button{margin-right:6px;}
.img_reply_btn{background:#e71d36;padding:6px 14px;color:#fff;}
.img_reply_btn_close{background:#cccccc;}
.img_reply_list{margin:25px 0;}
.img_reply_list img{margin-right:15px;}
.img_reply_item{margin-bottom:14px;}
.img_reply_item div.fl{position:relative;width:672px;background:#f6f6f6;padding:8px 10px;border:1px solid #dbdbdb;}
.img_reply_item .reply_margin{margin:14px 0 0 65px;}
.img_reply_item p{font:14px/16px "Microsoft Yahei";margin-bottom:10px;color:#636363;}
.img_reply_item p strong{color:#e71d36;margin-right:20px;font-weight:bold;}
.img_reply_item p .color{color:#e71d36;margin-right:20px;font-weight:bold;}
.img_reply_item p span{color:#999999;}
.img_reply_item p a{float:right;background:url(<%= asset_path "design/reply.gif" %>) no-repeat;padding-left:24px;height:17px;color:#565656;}
.reply_margin .reply_del{margin:0 20px;font-weight:normal;cursor:pointer;}
.imgbase_reply_arrow{position:absolute;width:14px;height:13px;background:url(<%= asset_path "imgbase/imgbase_reply_arrow.jpg" %>) no-repeat;left:-14px;top:20px;}
.imgbase_more_btn{padding:8px 0;text-align:center;background:#ececec;}
.imgbase_more_btn a{color:#6d6d6d;font:12px "SimSun";}

.author_show{float:left;width:365px;padding:18px 20px 0 0;}
.author_info{width:360px;border:1px solid #e6e6e6;}
.author_info img{padding:20px 35px;}
.author_info p{padding:22px 10px 10px;}
.author_info p strong{color:#e71d36;font:16px "Microsoft Yahei";display:block;margin:10px 0;}
a.img_reply_btn{display:inline-block;}
.author_info a:hover{color:#fff;}
.author_info a.imgbase_comm,.author_info a.imgbase_like{color:#7b7b7b;}
.author_info a.imgbase_comm:hover,.author_info a.imgbase_like:hover{color:#7b7b7b;}
.author_info a.imgbase_comm,.author_info a.imgbase_like{float:left;margin-left:0;margin-right:10px;}
.author_info div.fl{width:170px;min-height:159px;}
.author_info div.fl p{padding-bottom:5px;}
.imgs_share{padding:10px 0;margin:10px 0;}
.imgs_share span.fl{padding-top:4px;margin-right:10px;font-weight:bold;}
.imgs_otherinfo{margin:10px 0;padding:10px 0;}
.imgs_otherinfo h4{font-weight:bold;margin-bottom:10px;}
.imgs_otherinfo h4 span{font-weight:normal;}
.imgs_otherinfo span{margin-right:10px;}
.imgs_in_list dl{float:left;padding:5px 2px;width:178px;overflow:hidden;}
.imgs_in_list dl dt{font-weight:bold;margin-bottom:10px;}
.imgs_in_list dl dd{text-align:center;margin-bottom:5px;}
.imgs_in_list dl dd span{display:block;padding:5px 0;font:12px "SimSun";color:#707070;}
.imgs_banner{margin-top:15px;}


.border_bline{border-bottom:1px solid #e6e6e6;}
.border_lline{border-left:1px solid #e6e6e6;}
.nomargin{margin:0!important;}

.colorsbox .colors_con .desc .list li .colorblock{float:left;margin:0 5px 0 0;width:30px;height:30px}
.colorsbox .colors_con .desc .list li{ float:left; border-right:1px dotted #ccc; margin-right:10px; padding:0 10px;}
.colorsbox .colors_con .desc .list li h5{ color:#6d1718; font:12px "Microsoft Yahei";}
.colorsbox .colors_con .desc .list li p{ font:12px "Microsoft Yahei"; padding:0 5px 5px 35px;}

