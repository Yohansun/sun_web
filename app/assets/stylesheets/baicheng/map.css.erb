#map li ul{position: relative;display: none\9;z-index: 2;
	-webkit-transform:scale(0);
	-webkit-transition:all 0.5s;
	-moz-transform:scale(0);
	-moz-transition:all 0.5s;
	-o-transform:scale(0);
	-o-transition:all 0.5s;
	transform:scale(0);
	transition:all 0.5s;
}
#map li:hover ul,#map li ul:hover{display: block\9;
	-webkit-transform:scale(1);
	-moz-transform:scale(1);
	-o-transform:scale(1);
	transform:scale(1);
}

.map-ring{background: url(<%= asset_path 'baicheng/map/map-ring.png' %>) center center no-repeat;width: 214px;height: 212px;margin: -102px 0 0 -104px;}

.token{background: url(<%= asset_path 'baicheng/map/map-token.png' %>) no-repeat -43px 0;width: 36px;height: 36px;padding: 3px;}
#map li li:hover{background-position: 0 0;}
#map img{width: 36px;height: 36px;}
#map div{background-color: #2b2b2b;position: absolute;z-index: 3;padding: 10px;width: 70px;height: 35px;top: -63px;left: -23px;display: none;}
#map li li:hover div{display: block;}
#map div a{color: #e71d36;display: block;width: 70px;}
#map div a,#map p{overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
#map b{color: #909090;}
#map p{color: #fff;width: 90px;}
#map i{background: url(<%= asset_path 'baicheng/map/map-caret.png' %>) no-repeat;width: 10px;height: 8px;position: absolute;left: 39px;top: 55px;}

