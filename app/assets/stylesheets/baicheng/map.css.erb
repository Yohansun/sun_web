body{background-image: url(<%= asset_path 'baicheng/map/map-bg.jpg' %>);}
.map{background: url(<%= asset_path 'baicheng/map/map.jpg' %>) no-repeat;height: 900px;padding: 0;}
.map .refresh-logo{background: none;position: absolute;left: 0;bottom: 100px;z-index: 1;}
#map{width: 1080px;height: 900px;position: relative;left: 0;top: 0;font-size: 12px;}
#map li{position: absolute;cursor: pointer;}
#map > li{background: url(<%= asset_path 'baicheng/map/map-dot.jpg' %>) no-repeat;width: 9px;height: 9px;}
