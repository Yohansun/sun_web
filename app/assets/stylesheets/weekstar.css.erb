/* 每周之星 */
.weekstart_titleA,.weekstart_titleB,.weekstart_titleC{margin-bottom:15px;padding:50px 0 15px;height:25px;overflow:hidden;border-bottom:1px dotted #78796b}
.weekstart_titleA h1{margin:auto;width:645px;color:#e81e36;font-size:24px;font-weight:bold;text-align:center;line-height:40px}
.weekstart_titleA .subtitle{float:left;margin-top:15px;color:#5f5f5f;font-size:14px}
.weekstart_titleA .fullscreen_btn,
.weekstart_titleB .fullscreen_btn,
.weekstart_titleC .fullscreen_btn{float:right;width:112px;height:31px;text-indent:-1000em;background:url(<%= asset_path "design/full-screen-btn.gif"%>)}
.weekstart_titleA .fullscreen_btn a,
.weekstart_titleB .fullscreen_btn a,
.weekstart_titleC .fullscreen_btn a{display:block;height:100%}

.weekstart_titleB{height:45px}
.weekstart_titleB h1{margin-left:5px;color:#e81e36;font-size:24px;font-weight:bold}
.weekstart_titleB .subtitle{float:left;margin-top:10px;width:600px}
.weekstart_titleB .votes{float:left;margin-left:5px;height:20px;overflow:hidden;padding-left:18px;font-size:14px;color:#5c5c5c;font-weight:bold;background:url(<%= asset_path "design/d_icon.png"%>) no-repeat 0 5px}
.weekstart_titleB .votes img{vertical-align:top}
.weekstart_titleB .operate{font-size:12px}
.weekstart_titleB .operate span{float:left;margin-left:10px;padding-left:20px;color:#5C5C5C;line-height:18px;background:url(<%= asset_path "design/d_icon.png"%>) no-repeat left -35px}
.weekstart_titleB .operate .review{background-position:0 -16px}

.weekstart_titleC{height:48px}
.weekstart_titleC h1{float:left;padding-right:68px;font-size:36px;color:#de031f;background:url(<%= asset_path "design/best.gif"%>) no-repeat right}
.weekstart_titleC h1 p{margin-top:3px;color:#787878;font-family:tahoma;font-size:16px}
.weekstart_titleC .fullscreen_btn{margin-top:22px}

.weekstart_detail .gallery{position:relative;padding-top:34px;height:362px;background:url(<%= asset_path "design/weekstar-galleryA-bg.gif"%>) no-repeat}
.weekstart_detail .gallery span{display:block;position:absolute;top:175px;z-index:20;width:43px;height:65px;cursor:pointer;background:url(<%= asset_path "design/galleryK.png"%>)}
.weekstart_detail .gallery .moveleft{left:140px}
.weekstart_detail .gallery .moveright{right:140px;background-position:0 100%}
.weekstart_detail .galleryA{height:365px;position:relative}
.weekstart_detail .galleryA img{position:absolute}

.weekstart_detail .big_image{text-align:center;width:888px;/*height:590px;*/}
.big_image .pa{right:0;bottom:0;}

.color-picker{margin:35px 0 20px;padding:20px 12px 10px;border:1px solid #dedfe3}
.color-picker .overview{height:35px;border-bottom:1px solid #dadada}
.color-picker .votes{float:left;margin-left:5px;height:27px;overflow:hidden;padding-left:18px;font-size:18px;color:#5c5c5c;font-weight:bold;background:url(<%= asset_path "design/d_icon.png"%>) no-repeat 0 13px}
.color-picker .votes span{font-size:26px;color:#d52015;font-family:"Times New Roman", Times, serif}
.color-picker .operate{float:right;margin:10px 5px 0 0;font-size:12px}
.color-picker .operate span{float:left;margin-left:10px;padding-left:20px;color:#5C5C5C;line-height:18px;background:url(<%= asset_path "design/d_icon.png"%>) no-repeat left -35px}
.color-picker .operate .review{background-position:0 -16px}
.color-picker .color{margin-top:12px;height:46px;background:url(<%= asset_path "design/colors.gif"%>) no-repeat}
.color-picker .color .palettes{padding:6px 0 0 59px}
.color-picker .color .palette{float:left;margin-right:7px;width:71px;height:35px;position:relative}
.color-picker .color .palette .desc{display:none;position:absolute;left:-46px;bottom:20px;width:500px;padding-bottom:30px;background:url(<%= asset_path "design/popup_box.gif"%>) no-repeat right bottom}
.color-picker .color .palette:hover .desc{display:block}
.color-picker .color .palette .desc .title{height:7px;overflow:hidden;background:url(<%= asset_path "design/popup_box.gif"%>) no-repeat 0 0}
.color-picker .color .palette .desc .list{width:498px;overflow:hidden;border-left:1px solid #c6cbcf;border-right:1px solid #c6cbcf;background:#fefefe}
.color-picker .color .palette .desc .list ul{margin:10px 0 -10px 10px;width:110%;zoom:1}
.color-picker .color .palette .desc .list ul:after{content:'';display:block;clear:left;font-size:0}
.color-picker .color .palette .desc .list li{float:left;margin:0 20px 0 0;padding:0 20px 5px 2px;width:130px;height:33px;border-right:1px dotted #dcdcdc;font-size:12px}
.color-picker .color .palette .desc .list li .colorblock{float:left;margin:0 5px 0 0;width:30px;height:30px}
.color-picker .color .palette .desc .list li h5{color:#6d1718}
.color-picker .color .palette .desc .list li p{margin-top:5px;padding-bottom:5px;border-bottom:1px solid #ccc}
.color-picker .gotaobao{float:right;width:184px;height:38px}
.color-picker .gotaobao a{display:block;height:100%;text-indent:-1000em}

.weekstart_detail .galleryB{margin:20px 0}
.weekstart_detail .properties{margin:20px 10px}
.weekstart_detail .review_form{margin:auto;width:815px}
.weekstart_detail .pager{margin:10px 0;width:auto;text-align:right}
.weekstart_detail .review_wrap{padding-bottom:30px}
.weekstart_detail .properties .tags{margin-bottom:5px;font-size:12px}
.weekstart_detail .properties .tags h5, .weekstart_detail .properties .tags ul, .weekstart_detail .properties .tags li{display:inline}
.weekstart_detail .properties .tags h5{padding-left:16px;background:url(<%= asset_path "design/tagA.gif"%>) no-repeat 0 center}
.weekstart_detail .properties .tags li{margin-left:10px}
.weekstart_detail .properties .tags a{color:#ea0100;text-decoration:underline}


.galleryB{position:relative;height:160px;border-top:1px dotted #737373;border-bottom:1px dotted #737373}
.galleryB span{position:absolute;top:35px;display:block;width:20px;height:76px;cursor:pointer;background:url(<%= asset_path "design/galleryB.gif"%>)}
.galleryB .moveleft{left:0}
.galleryB .moveright{right:0;background-position:0 100%}
.galleryB .list{position:absolute;left:27px;top:10px;width:850px;height:138px;overflow:hidden}
.galleryB .list ul{position:absolute;left:0;top:0;width:10000px;height:100%}
.galleryB .list li{float:left;margin-right:10px}
.galleryB .list li img{width:205px;height:138px}

.design_listA{width:900px;overflow:hidden}
.design_listA ul{margin:20px;width:110%}
.design_listA li{float:left;margin:0 15px 20px 0;width:205px;height:225px;overflow:hidden;border-bottom:1px dotted #A09F9D}
.design_listA h3{border-bottom:1px dotted #a09f9d; font:14px "Microsoft Yahei"; color:#313131; font-weight:bold;margin-top:5px; padding-bottom:7px;}
.design_listA p,.inspiration_result .row p{overflow:hidden; margin-top:5px; font-size:12px; color:#666; line-height:20px;height:20px}
.design_listA p strong,.design_listA p span,.inspiration_result p strong,.inspiration_result .row p span{ float:left;color:#5c5c5c; padding-left:20px;display:block; margin-right:10px; line-height:18px;}
.design_listA p.p1 strong,.inspiration_result p.p1 strong{ background:url(<%= asset_path "design/d_icon.png"%>) no-repeat left 2px;}
.design_listA p.p2 span.zf,.inspiration_result p.p2 span.zf{ background:url(<%= asset_path "design/d_icon.png"%>) no-repeat left -35px;}
.design_listA p.p2 span.pl,.inspiration_result p.p2 span.pl{ background:url(<%= asset_path "design/d_icon.png"%>) no-repeat left -16px;}
.design_listA li img{vertical-align:top}

.propertyA a{color:#a66f36;text-decoration:underline}
.propertyA th,.propertyA td{vertical-align:top;line-height:1.8}
.propertyA th{white-space:nowrap;text-align:left;}

.reviewform{border-top:1px solid #dbdad8}
.reviewform .bar{padding:0 10px;height:32px;line-height:32px;color:#8a8987;font-size:14px;background:#f0fbfd}
.reviewform .bar h4{float:left;line-height:32px}
.reviewform .bar h4 span{font-weight:bold}
.reviewform .bar .r{float:right}
.reviewform .button{margin-top:10px}
.reviewform textarea{width:893px;height:100px;padding:5px;border:1px solid #e3e3e3}

.review_listA li{zoom:1;vertical-align:top}
.review_listA li:after{content:'';display:block;clear:both}
.review_listA .bar{padding:0 18px;height:22px;border-top:1px solid #dcdcdc;line-height:22px;font-size:14px;background:#f3f3f3}
.review_listA .bar .user{float:left;font-weight:bold;color:#333}
.review_listA .bar .time{float:right;color:#909090}
.review_listA .content{float:left;width:750px;padding:25px 0 25px 18px;font-size:14px}
.review_listA .reply{float:right;display:inline}
.review_listA .reply a{display:block;margin:25px 18px;padding-left:24px;padding-bottom:10px;height:20px;color:#565656;font-size:14px;background:url(<%= asset_path "design/reply.gif"%>) no-repeat 0 2px}
* html .review_listA .reply a{width:1%;white-space:nowrap}


@charset "utf-8";

/* galleryK Settings
==================================== */
@media screen, projection {
	.galleryK {
		overflow:hidden;
		position:relative;
		text-align:left;
		visibility:hidden;
		width:100%;
	}
	.galleryK img {
		border:none;
		position:absolute;
		top:0px;
		visibility:hidden;
		-ms-interpolation-mode:bicubic; /* Enables bicubic image resampling for the IE7 */
	}
	.galleryK p {
		margin:0 auto;
		text-align:center;
	}
	.galleryK .loading {
		border:1px solid white;
		height:15px;
		left:50%;
		margin-left:-106px;
		padding:5px;
		position:relative;
		visibility:visible;
		width:200px;
	}
	.galleryK .loading_bar {
		background:#fff;
		height:15px;
		visibility:visible;
		width:1%;
	}
	.galleryK .navigation{
		z-index:10000;
	}
	.galleryK .caption {
		font-weight:bold;
		position:relative;
		text-align:center;
		z-index:10001;
	}
	.galleryK .scrollbar {
		border-bottom:1px solid #b3b3b3;
		position:relative;
		visibility:hidden;
		z-index:10002;
		height:1px;
	}
	.galleryK .slider {
		background:url(slider.png) no-repeat;
		height:14px;
		margin:-6px 0 0 -7px;
		position:absolute;
		width:14px;
		z-index:10003;
	}
	.galleryK .slideshow {
		cursor:pointer;
		height:14px;
		margin:20px 0 0 20px;
		position:absolute;
		width:14px;
		z-index:10003;
	}
	.galleryK .slideshow.pause {
		background:url(button_pause.png) no-repeat;
	}
	.galleryK .slideshow.play {
		background:url(button_play.png) no-repeat;
	}
	.galleryK .images {
		overflow:hidden;
		white-space:nowrap;
	}
	.galleryK .button {
		cursor:pointer;
		height:17px;
		position:relative;
		width:17px;
	}
	.galleryK .previous {
		background: url(button_left.png) top left no-repeat;
		float:left;
		margin: -7px 0 0 -30px;
	}
	.galleryK .next {
		background: url(button_right.png) top left no-repeat;
		float:right;
		margin:-7px -30px 0 30px;
	}
