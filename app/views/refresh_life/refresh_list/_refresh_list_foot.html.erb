<div class="index_main refresh_news mt10 clearfix">
  <div class="refresh_new pull-left">
    <h5><a href="<%= @design_more.try(:url) %>" target="_blank" class="pull-right"><small class="muted">更多></small></a>装修图库精选</h5>
    <ul class="ul_dot">
      <% if @design_datas.present? %>
        <% @design_datas.each do |data| %>
          <% unless data.position == 0 %>
            <li><a href="<%= data.url %>" target="_blank"><%= data.title %></a></li>
          <% end %>
        <% end %>
      <% end %>
    </ul>
  </div>

  <div class="refresh_new pull-left">
    <h5><a href="<%= @weekly_star_more.try(:url) %>" target="_blank" class="pull-right"><small class="muted">更多></small></a>设计之星作品精选</h5>
    <ul class="ul_dot">
      <% if @weekly_star_datas.present? %>
        <% @weekly_star_datas.each do |data| %>
          <% unless data.position == 0 %>
            <li><a href="<%= data.url %>" target="_blank"><%= data.title %></a></li>
          <% end %>
        <% end %>
      <% end %>
    </ul>
  </div>

  <div class="pull-right"><a href="<%= @list_banner3.url? ? @list_banner3.url : 'javascript:void(0);' %>" target="_blank"><img src="<%=@list_banner3.try(:file).try(:url,:list_230x230)%>"></a></div>
</div>
<div class="clearfix">
  <div class="footer_banner pull-left"><a href="<%= @list_banner4.url? ? @list_banner4.url : 'javascript:void(0);' %>" target="_blank"><img src="<%=@list_banner4.try(:file).try(:url,:list_670x85)%>"></a></div>
  <div class="footer_banner pull-right"><a href="<%= @list_banner5.url? ? @list_banner5.url : 'javascript:void(0);' %>" target="_blank"><img src="<%=@list_banner5.try(:file).try(:url,:list_310x85)%>"></a></div>
</div>