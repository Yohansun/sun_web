<ul class="unstyled map-right index_main mt10 js-img_show">
  <li class="clearfix refresh_r_tit">
    <a href="#" target="_blank" class="pull-right"><small class="muted">更多&gt;</small></a>
    <strong class="pull-left">最新大师作品</strong>
  </li>
  <% @new_designs.each_with_index do |_design,index| %>
    <li class="map-r1 active <%= index == 0 ? 'dott1' : ''%>">
      <i class="icon-i16 icon-1"><%= index+1 %></i>
      <strong class="js-title"><%= _design.design_name %></strong>
      <% if index == 0 %>
      <% cover = _design.cover_design_upload %>
      <a href="<%= dialog_celebrity_master_design_path(_design,upload_id: cover.id ) if cover.present? -%>" class="js-img"><img src="<%=cover.try(:file2,:interview)%>" alt=""></a>
      <% end -%>
    </li>
  <%end-%>
</ul>