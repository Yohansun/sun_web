<div class="content">
  <div class="title"><h5> 外链统计 </h5></div>
  <!-- Dynamic table -->
  <div class="table">
    <div class="head">
    	<h5 class="iFrames">外链统计列表</h5>
  	</div>
  	 	    <%= form_tag("/admin/content/lands", :method => "get") do %>
				<%#= select_tag "source", options_for_select(Land::SOURCE, "请选择分类")  %>
    		<label>开始时间</label><%= text_field_tag "begin_at", (Time.now).strftime("%Y-%m-%d") %>
    		<label>结束时间</label><%= text_field_tag "end_at", (Time.now + 1.day).strftime("%Y-%m-%d") %>
    		<%= submit_tag "查询" %>
	    <%- end -%>

    <table cellpadding="0" cellspacing="0" border="0" class="display" id="example">
      <thead>
        <tr>
          <th width="50%">source</th>
          <th width="50%">数量</th>
        </tr>
      </thead>
      <tbody>
        <%- collection.each do |c| %>
          <tr class="gradeA">
            <td><%= c.source %></td>
            <td><%= c.count %></td>
</td>
          </tr>
        <%- end %>
      </tbody>
    </table>
  </div>
</div>
<label></label>