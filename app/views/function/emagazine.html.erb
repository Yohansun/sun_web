<style type="text/css">
/* =====  CSS reset  ===== */
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td { margin: 0; padding: 0; border: 0; outline: 0; font-size: 100%; vertical-align: baseline; background: transparent; }
ol, ul { list-style: none; }
blockquote, q { quotes: none; }
blockquote:before, blockquote:after, q:before, q:after { content: ''; content: none; }
/* remember to define focus styles! */
:focus { outline: 0; }
/* remember to highlight inserts somehow! padding: 9px 17px 9px 17px; */
ins { text-decoration: none; }
del { text-decoration: line-through; }
/* tables still need 'cellspacing="0"' in the markup */
table { border-collapse: collapse; }
*, * focus { outline: none; margin: 0; padding: 0; }
textarea { overflow: auto; }
textarea, input[type=text], input[type=password] { font-family: Arial, Helvetica, sans-serif; font-size: 11px; }
button, input[type=submit] { font-family: Arial, Helvetica, sans-serif; }
img { border: 0; }
ul { list-style: none; margin: 0; padding: 0; }
p { margin: 0; padding: 0; }
:focus { outline: 0; }
a { text-decoration: none; color: #2b6893; }
.normal { font-style: normal; }
.normalFont { font-style: normal; font-weight: normal; }
.nomargin { margin: 0!important; }
input::-moz-focus-inner /*Remove button padding in FF*/ { border: 0; padding: 0;}
 button::-moz-focus-inner { padding: 0; border: 0}
 a.button::-moz-focus-inner /*Remove button padding in FF*/ { border: 0; padding: 0;}
/**/
body { background:url(<%= asset_path "bg.jpg"%>) no-repeat top center; }
.shadow { zoom: 1; }

/*edm_header*/
.edm_header { text-align:center; }


/*emagazine*/
.emagazine{ background:url(<%= asset_path "emagezine/emagazine_bg1.png"%>) no-repeat top center; padding-top:10px;}
.emagazine .e_new{ overflow:hidden; background:#fff;width:771px; margin:0 auto; padding:30px 15px 15px;}
.new_act_box,.new_news_box{width:381px;}
.emagazine h2{ padding:5px 0px; margin-bottom:5px; }
.act_img{ background:#e71d35; padding:6px 6px 10px; margin-top:17px;}
.act_img p{ font:14px/24px "Microsoft Yahei";}
.e_act_btn{ margin:16px 5px 0 0;}
.new_news{ overflow:hidden; padding:12px;}
.new_news div.fr{ width:260px;}
.new_news div.fr h3 a{color:#710514; font:14px "Microsoft Yahei";}
.new_news div.fr p{font:12px/20px "Microsoft Yahei";color:#606060;}
.new_news div.fr p a{color:#920015;}
.new_news_box .new_news p.pl40{background:url(<%= asset_path "master/tag_icon.png"%>) no-repeat left center;font:13px "arial simsun";color:#e9700f;margin-top:3px; padding-left:40px;}

.emagazine .week_info{ background: url(<%= asset_path "emagezine/emagazine_bg2.png"%>) no-repeat top center;padding-top:31px;width:801px; margin:0 auto;}
.emagazine .week_info .bg_fff{overflow:hidden; background:#fff;width:722px; margin:0 auto; padding:5px 5px 5px 20px;}
.emagazine .week_info .star_img{ position:relative; background: url(<%= asset_path "emagezine/week_star_bg.png"%>) no-repeat left 0; width:240px; height:224px; padding:4px 4px 50px 20px; z-index:9999;}
.emagazine .week_info .star_img a{ display:block;}
.emagazine .week_info .star_img img{ float:none;}
.emagazine .week_info .week_star p{color:#fff; font:14px "Microsoft Yahei";}
.emagazine .week_info .week_star p.xin_bg{ padding-left:20px; background:url(<%= asset_path "emagezine/xin_bg.png"%>) no-repeat 1px center;}
.week_star_icon{ position:absolute;width:53px; height:53px; top:0px; right:1px;}

.emagazine .master_talk{ margin-left:25px; position:relative; z-index:9999;}
.master_talk .list_main{ padding:5px 0 0;width:411px; height:267px; background:url(<%= asset_path "emagezine/talk_bg2.png"%>) no-repeat center 0; border:none;}
.master_talk .list_main .img_name{ height:221px;margin:0 5px;}
.master_talk .list_main .img_name h2{width:211px!important; height:40px; padding:5px 0 5px 5px; margin:0; bottom:50px; line-height:18px; font-size:12px; font-weight:normal; text-align:left;}
.master_talk .list_main h3{ color:#fff; font:40px "Myriad Pro,Microsoft Yahei";}
.master_talk .list_main p{ color:#fff; font:12px "Microsoft Yahei";}
.master_talk .list_main p a{ color:#fff; font:12px "Microsoft Yahei";}
/* magazine_bottom*/
.magazine_bottom{ position:relative; top:-20px;}
.magazine_bottom img{ display:block; margin:0 auto;}
.magazine_bottom .bg_fff{ background:#fff; width:873px; margin:0 auto; padding:10px; overflow:hidden;}
.magazine_bottom .magazine_tips{ width:260px; background:url(<%= asset_path "emagezine/shadow_r.png"%>) no-repeat right 10px; padding:0px 10px 10px 0;}
.magazine_bottom .magazine_tips .interview{ margin:0; padding:0px; border:none;}
.magazine_bottom .magazine_tips .interview h3{ margin-bottom:5px; overflow:hidden; color:#666666;font:12px/21px "arial simsun";}
.magazine_bottom .magazine_tips .interview  span{ float:right;}

.magazine_bottom .ins_works{ width:335px; background:url(<%= asset_path "emagezine/shadow_r.png"%>) no-repeat right 10px;}
.magazine_bottom .ins_works h2{margin-left: 15px;margin-right: 15px;}
.magazine_bottom .ins_works ul{ overflow:hidden;}
.magazine_bottom .ins_works ul li{ float:left; display:block; width:147px; padding:5px 10px;}
.magazine_bottom .ins_works ul li img{ padding:1px; border:1px solid #ccc; margin:0 auto;}
.magazine_bottom .ins_works ul li h3{ font:12px "arial simsun"; line-height:24px; color:#666;}


.ins_works p strong,.ins_works p span{float:left;color:#5c5c5c; padding-left:16px; margin-right:0px; margin-top:5px;display:block;height:17px; font:12px/17px "arial simsun"; font-weight: normal;}
.ins_works .p1 strong{ background:url(<%= asset_path "design/d_icon.png"%>) no-repeat left 2px;}
.ins_works p.p2 span.zf{ background:url(<%= asset_path "design/d_icon.png"%>) no-repeat left -36px;}
.ins_works p.p2 span.pl{ background:url(<%= asset_path "design/d_icon.png"%>) no-repeat left -17px;}


.e_works { width:260px;}
.e_works img { margin:1px;}
.e_works ul{ overflow:hidden; width:260px;}
.e_works li { float:left;}
.e_btn{ text-align:center; width:389px; height:50px; margin:25px auto; border:1px solid #ccc;background:#f4f4f4; padding:10px;}
.e_btn a{ display:block; border:10px solid; border-color:#cbcbcb #d7d7d7; height:30px;font:16px/30px "Microsoft Yahei"; color:#000;}
.e_copyright{width:885px; height:36px; margin:0 auto; padding:15px 10px; background:#000;}
.e_copyright,.e_copyright a,.e_copyright p{ color:#909090; font:12px "Microsoft Yahei";}

.list_btn{text-align:center;}
.list_btn a img{margin:25px 15px;}
.list_main{ overflow:hidden; padding-bottom:10px; border-bottom:5px solid #e9e9e9;}
.list_main .img_name{float:left; height:300px; overflow:hidden;}
.list_main .img_name h2{position:relative; bottom:50px;height:50px; width:278px; background:url(<%= asset_path "master/meng1.png"%>) repeat-x 0 0; text-align:center; line-height:50px; color:#fff; font-size:20px;font-weight:bold; font-family:"Microsoft Yahei";}
.list_detail{ float:right; width:610px;}
.list_detail h3 a{color:#212121;font:bold 20px "Microsoft Yahei";}
.list_detail p{color:#3f3f3f;font:14px/28px "Microsoft Yahei";}
.list_detail p.h3_sub{ background:none; line-height:16px;}
.list_detail p.h3_sub{ padding:4px 2px 10px; border-bottom:1px solid #ececec;}

.interview_box{overflow:auto;max-height:948px;}
.interview {margin:24px;border-bottom:1px solid #e0e0e0;padding-bottom:20px;}
.interview h3{padding-left:20px;background:url(<%= asset_path "master/question_mark_icon.png"%>) no-repeat left 4px;font:14px "Microsoft Yahei";color:#ba0000;margin-bottom:20px;}


/**/
.fl { float:left; display:inline; }
.fr { float:right; display:inline; }
.line_b_dot{border-bottom: 1px dotted #949494;}

</style>
